{
	"name": "alitrip.travel.trade.template.query",
	"apiChineseName": "订单服务详情模版查询",
	"description": "通过订单ID获取标注模版信息，商家可以根据模版来填充行业信息",
	"requestParams": [
		{
			"name": "is_new",
			"type": "Boolean",
			"description": "是否取最新的模版"
		},
		{
			"name": "order_id",
			"type": "Number",
			"description": "淘宝平台订单ID",
			"required": true
		}
	],
	"responseParams": [
		{
			"name": "result",
			"type": "ResultSet",
			"description": "订单服务标注模版获取结果",
			"subParams": [
				{
					"name": "first_result",
					"type": "OrderTipInfoResult",
					"description": "订单服务标注模版获取结果详情",
					"required": true,
					"subParams": [
						{
							"name": "is_new",
							"type": "Boolean",
							"description": "是否是最新的模版，如果订单没有标注模版，则默认取最新的模版数据；如果订单有标注模版，则取对应的模版信息；",
							"required": true
						},
						{
							"name": "template_info",
							"type": "OrderTemplateInfo",
							"description": "模版详情",
							"required": true,
							"subParams": [
								{
									"name": "category_id",
									"type": "Number",
									"description": "模版对应的类目ID",
									"required": true
								},
								{
									"name": "model_list",
									"type": "OrderTipFormInfo[]",
									"description": "填充字段列表",
									"required": true,
									"subParams": [
										{
											"name": "accept_types",
											"type": "String[]",
											"description": "字段可接收数据类型",
											"required": true
										},
										{
											"name": "desc",
											"type": "String",
											"description": "字段具体描述",
											"required": true
										},
										{
											"name": "max_num",
											"type": "Number",
											"description": "最大数量 (如最大文件上传数量, 最大图片上传数量等)",
											"required": true
										},
										{
											"name": "options",
											"type": "OrderTipOptionInfo[]",
											"description": "选择项, 当type = select / selects 的时候存在",
											"required": true,
											"subParams": [
												{
													"name": "text",
													"type": "String",
													"description": "select对应的text值",
													"required": true
												},
												{
													"name": "value",
													"type": "String",
													"description": "select对应的value值",
													"required": true
												}
											]
										},
										{
											"name": "rule",
											"type": "OrderTipRuleInfo",
											"description": "字段匹配规则",
											"required": true,
											"subParams": [
												{
													"name": "msg",
													"type": "String",
													"description": "字段描述",
													"required": true
												},
												{
													"name": "regex",
													"type": "String",
													"description": "字段正则表达式，字段值必须和正则匹配，否则上传服务信息会报错",
													"required": true
												},
												{
													"name": "require",
													"type": "Boolean",
													"description": "字段是否必须",
													"required": true
												}
											]
										},
										{
											"name": "title",
											"type": "String",
											"description": "字段中文名称",
											"required": true
										},
										{
											"name": "name",
											"type": "String",
											"description": "字段属性名，在做服务信息回传时需要用到的KEY",
											"required": true
										},
										{
											"name": "value",
											"type": "String",
											"description": "字段对应的值，单选值",
											"required": true
										},
										{
											"name": "values",
											"type": "String[]",
											"description": "字段对应的值，多选值",
											"required": true
										}
									]
								},
								{
									"name": "template_id",
									"type": "Number",
									"description": "模版对应ID",
									"required": true
								},
								{
									"name": "version",
									"type": "Number",
									"description": "模版对应版本",
									"required": true
								}
							]
						},
						{
							"name": "view_template_url",
							"type": "String",
							"description": "查看模版链接",
							"required": true
						}
					]
				}
			]
		}
	]
}