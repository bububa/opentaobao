{
	"name": "taobao.antifraud.riskuser.get",
	"apiChineseName": "反欺诈用户风险查询",
	"description": "根据用户基础信息，核实平台上的用户是否存在欺诈风险",
	"requestParams": [
		{
			"name": "param_account_query",
			"type": "ParamAccountQuery",
			"description": "风险用户查询条件",
			"subParams": [
				{
					"name": "app_key",
					"type": "String",
					"description": "反欺诈服务AppKey",
					"required": true
				},
				{
					"name": "timestamp",
					"type": "String",
					"description": "时间戳",
					"required": true
				},
				{
					"name": "app_token",
					"type": "String",
					"description": "身份校验信息",
					"required": true
				},
				{
					"name": "scene_id",
					"type": "String",
					"description": "场景ID",
					"required": true
				},
				{
					"name": "phone_number",
					"type": "String",
					"description": "手机号",
					"required": true
				},
				{
					"name": "ip",
					"type": "String",
					"description": "IP",
					"required": true
				},
				{
					"name": "trans",
					"type": "String",
					"description": "透传参数，是一个json形式的字符串"
				}
			]
		}
	],
	"responseParams": [
		{
			"name": "event_id",
			"type": "String",
			"description": "服务调用成功时, 返回的系统流水号"
		},
		{
			"name": "score",
			"type": "String",
			"description": "风险分值"
		},
		{
			"name": "risk_level",
			"type": "String",
			"description": "风险结果, 为reject, review, pass三者之一"
		},
		{
			"name": "detail_list",
			"type": "AccountRiskDetail []",
			"description": "风险结果详情列表，包含多个风险结果单项",
			"subParams": [
				{
					"name": "name",
					"type": "String",
					"description": "表示一个具体的用户风险详情"
				}
			]
		}
	]
}