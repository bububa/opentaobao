{
	"name": "tmall.exchange.messages.get",
	"apiChineseName": "查询换货订单留言列表",
	"description": "查询换货订单留言列表",
	"requestParams": [
		{
			"name": "operator_roles",
			"type": "Number[]",
			"description": "留言创建角色。具体包括：卖家主账户(1)、卖家子账户(2)、小二(3)、买家(4)、系统(5)、系统超时(6)",
			"maxListSize": 20
		},
		{
			"name": "page_size",
			"type": "Number",
			"description": "每页条数"
		},
		{
			"name": "dispute_id",
			"type": "Number",
			"description": "换货单号ID",
			"required": true
		},
		{
			"name": "page_no",
			"type": "Number",
			"description": "页码"
		},
		{
			"name": "fields",
			"type": "String[]",
			"description": "返回的字段。具体包括：id,refund_id,owner_id,owner_nick,owner_role,content,pic_urls,created,message_type",
			"required": true,
			"maxListSize": 20
		}
	],
	"responseParams": [
		{
			"name": "result",
			"type": "RefundMessageResult",
			"description": "返回结果",
			"subParams": [
				{
					"name": "page_results",
					"type": "Number",
					"description": "当前页面的留言条数",
					"required": true
				},
				{
					"name": "results",
					"type": "RefundMessage[]",
					"description": "留言记录",
					"required": true,
					"subParams": [
						{
							"name": "id",
							"type": "Number",
							"description": "留言ID",
							"required": true
						},
						{
							"name": "refund_id",
							"type": "Number",
							"description": "换货单号ID",
							"required": true
						},
						{
							"name": "owner_id",
							"type": "Number",
							"description": "留言者ID",
							"required": true
						},
						{
							"name": "owner_nick",
							"type": "String",
							"description": "留言者昵称",
							"required": true
						},
						{
							"name": "content",
							"type": "String",
							"description": "留言内容",
							"required": true
						},
						{
							"name": "pic_urls",
							"type": "PicUrl[]",
							"description": "凭证信息",
							"required": true,
							"subParams": [
								{
									"name": "url",
									"type": "String",
									"description": "凭证图片url",
									"required": true
								}
							]
						},
						{
							"name": "created",
							"type": "Date",
							"description": "留言创建时期",
							"required": true
						},
						{
							"name": "message_type",
							"type": "String",
							"description": "留言类型",
							"required": true
						},
						{
							"name": "owner_role",
							"type": "String",
							"description": "留言角色",
							"required": true
						}
					]
				},
				{
					"name": "total_results",
					"type": "Number",
					"description": "所有留言记录数",
					"required": true
				},
				{
					"name": "has_next",
					"type": "Boolean",
					"description": "是否有下一页",
					"required": true
				},
				{
					"name": "exception",
					"type": "Json",
					"description": "异常信息",
					"required": true
				},
				{
					"name": "error_code",
					"type": "String",
					"description": "错误码",
					"required": true
				},
				{
					"name": "error_msg",
					"type": "String",
					"description": "错误信息",
					"required": true
				}
			]
		}
	]
}