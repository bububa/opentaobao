					"required": true
				},
				{
					"name": "state",
					"type": "String",
					"description": "收货人的所在省份",
					"required": true
				},
				{
					"name": "country",
					"type": "String",
					"description": "收货人国籍",
					"required": true
				},
				{
					"name": "matched",
					"type": "Boolean",
					"description": "oaid是否和tid当前的oaid匹配。true：匹配，false：不匹配。当不匹配时，建议通过taobao.trade.fullinfo.get获取最新的oaid。",
					"required": true
				}
			]
		}
	]
