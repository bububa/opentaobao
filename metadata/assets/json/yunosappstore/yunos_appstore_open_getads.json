					]
				},
				{
					"name": "creatives",
					"type": "String[]",
					"description": "创意列表",
					"required": true
				},
				{
					"name": "deeplink",
					"type": "String",
					"description": "deeplink",
					"required": true
				},
				{
					"name": "download_url",
					"type": "String",
					"description": "下地地址",
					"required": true
