										{
											"name": "domestic",
											"type": "Number",
											"description": "0国内1国外",
											"required": true
										},
										{
											"name": "city_url",
											"type": "String",
											"description": "城市图片url",
											"required": true
										},
										{
											"name": "city_py_head",
											"type": "String",
											"description": "城市拼音首字母",
											"required": true
										},
										{
											"name": "city_code",
											"type": "Number",
											"description": "城市编码",
											"required": true
										},
										{
											"name": "country_code",
											"type": "Number",
											"description": "国家编码",
											"required": true
										},
										{
											"name": "city_cn",
											"type": "String",
											"description": "城市",
											"required": true
										},
										{
											"name": "country_cn",
											"type": "String",
											"description": "国家",
											"required": true
										}
									]
								},
								{
									"name": "letter",
									"type": "String",
									"description": "字母",
									"required": true
								}
							]
						},
						{
							"name": "hot_city_list",
							"type": "AddressSearchDto[]",
							"description": "热门城市",
							"required": true,
							"subParams": [
								{
									"name": "domestic",
									"type": "Number",
									"description": "0国内1国外",
									"required": true
								},
								{
									"name": "city_url",
									"type": "String",
									"description": "城市图片url",
									"required": true
								},
								{
									"name": "city_py_head",
									"type": "String",
									"description": "城市拼音首字母",
									"required": true
								},
								{
									"name": "city_code",
									"type": "Number",
									"description": "城市编码",
									"required": true
								},
								{
									"name": "country_code",
									"type": "Number",
									"description": "国家编码",
									"required": true
								},
								{
									"name": "city_cn",
									"type": "String",
									"description": "城市",
									"required": true
								},
								{
									"name": "country_cn",
									"type": "String",
									"description": "国家",
									"required": true
								}
							]
						}
					]
				}
			]
		}
