		{
			"name": "tenant_key",
			"type": "String",
			"description": "商家租户id",
			"required": true
		},
		{
			"name": "domestic",
			"type": "Number",
			"description": "0国内 1国外"
		}
	],
	"responseParams": [
		{
			"name": "result",
			"type": "Response",
			"description": "默认描述",
			"subParams": [
				{
					"name": "success",
					"type": "Boolean",
					"description": "是否成功",
					"required": true
				},
				{
					"name": "error_code",
					"type": "String",
					"description": "错误编码",
					"required": true
				},
				{
					"name": "error_msg",
					"type": "String",
