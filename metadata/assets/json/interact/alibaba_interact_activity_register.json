{
	"id": 23792,
	"name": "alibaba.interact.activity.register",
	"apiChineseName": "ISV互动应用活动注册服务",
	"description": "为支持卖家由ISV互动应用可以在手淘店铺首页透出，提供ISV互动应用创建的活动注册到手淘的服务",
	"requestParams": [
		{
			"name": "entry_url",
			"type": "String",
			"description": "页面内容链接，仅允许ascii字符",
			"required": true
		},
		{
			"name": "biz_id",
			"type": "String",
			"description": "活动ID",
			"required": true
		},
		{
			"name": "description",
			"type": "String",
			"description": "活动描述文字"
		},
		{
			"name": "end_time",
			"type": "Date",
			"description": "活动结束时间"
		},
		{
			"name": "name",
			"type": "String",
			"description": "活动名称",
			"required": true
		},
		{
			"name": "picture",
			"type": "String",
			"description": "活动封面图片（非必填）"
		},
		{
			"name": "start_time",
			"type": "Date",
			"description": "活动开始时间"
		},
		{
			"name": "has_valid_time",
			"type": "Boolean",
			"description": "是否有有效时间，若为真开始时间和结束时间必填，默认为真",
			"required": true
		}
	],
	"responseParams": [
		{
			"name": "register_sucess_info",
			"type": "AllsparkResult",
			"description": "活动注册成功，将活动注册后的ID和h5链接返回给调用方",
			"subParams": [
				{
					"name": "err_msg",
					"type": "String",
					"description": "出错提示"
				},
				{
					"name": "err_code",
					"type": "String",
					"description": "出错代码"
				},
				{
					"name": "data",
					"type": "ActivityWriteResult",
					"description": "服务结果对象",
					"subParams": [
						{
							"name": "activity_id",
							"type": "Number",
							"description": "注册成功ID"
						},
						{
							"name": "h5_url",
							"type": "String",
							"description": "活动页面h5链接"
						}
					]
				},
				{
					"name": "success",
					"type": "Boolean",
					"description": "是否注册成功"
				}
			]
		}
	]
}