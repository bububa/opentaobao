{
	"name": "cainiao.consignplatform.order.create",
	"apiChineseName": "菜鸟发货工作台创建订单",
	"description": "菜鸟发货工作台，商家或者isv通过api进行订单写入操作",
	"requestParams": [
		{
			"name": "create_request",
			"type": "OrderCreateRequest",
			"description": "订单创建入参",
			"required": true,
			"subParams": [
				{
					"name": "user_memo",
					"type": "String",
					"description": "商家备注"
				},
				{
					"name": "receive_address",
					"type": "AddressDtoForTop",
					"description": "收件地址",
					"required": true,
					"subParams": [
						{
							"name": "country_name",
							"type": "String",
							"description": "国家",
							"required": true
						},
						{
							"name": "prov_name",
							"type": "String",
							"description": "省份",
							"required": true
						},
						{
							"name": "city_name",
							"type": "String",
							"description": "城市",
							"required": true
						},
						{
							"name": "area_name",
							"type": "String",
							"description": "区",
							"required": true
						},
						{
							"name": "town_name",
							"type": "String",
							"description": "街道"
						},
						{
							"name": "address_detail",
							"type": "String",
							"description": "详细地址",
							"required": true
						}
					]
				},
				{
					"name": "receiver",
					"type": "PersonDto",
					"description": "收件人",
					"required": true,
					"subParams": [
						{
							"name": "phone",
							"type": "String",
							"description": "收件固定电话"
						},
						{
							"name": "mobile_phone",
							"type": "String",
							"description": "收件电话",
							"required": true
						},
						{
							"name": "user_name",
							"type": "String",
							"description": "收件人",
							"required": true
						}
					]
				},
				{
					"name": "buyer_memo",
					"type": "String",
					"description": "买家留言"
				},
				{
					"name": "outer_order_id",
					"type": "String",
					"description": "外部订单id，会用来做幂等，需要保证唯一",
					"required": true
				},
				{
					"name": "order_source",
					"type": "String",
					"description": "订单来源",
					"required": true
				},
				{
					"name": "sub_order_list",
					"type": "SubOrderDto[]",
					"description": "子订单列表",
					"subParams": [
						{
							"name": "outer_order_id",
							"type": "String",
							"description": "外部子订单id",
							"required": true
						},
						{
							"name": "amount",
							"type": "Number",
							"description": "商品数量",
							"required": true
						},
						{
							"name": "price",
							"type": "Number",
							"description": "商品价格（单位分）"
						},
						{
							"name": "picture_url",
							"type": "String",
							"description": "商品图片链接"
						},
						{
							"name": "category",
							"type": "Number",
							"description": "商品类别。1 日用品; 2 食品; 3 文件; 4 衣物; 5 数码产品; 6 其他",
							"required": true
						},
						{
							"name": "goods_name",
							"type": "String",
							"description": "商品名称",
							"required": true
						},
						{
							"name": "goods_id",
							"type": "String",
							"description": "商品id",
							"required": true
						}
					]
				}
			]
		}
	],
	"responseParams": [
		{
			"name": "fail_message",
			"type": "String",
			"description": "失败信息"
		},
		{
			"name": "fail_code",
			"type": "String",
			"description": "失败code"
		},
		{
			"name": "result",
			"type": "Boolean",
			"description": "创建是否成功"
		}
	]
}