{
	"name": "alibaba.seaking.diagnosistitle",
	"apiChineseName": "标题诊断",
	"description": "标题诊断",
	"requestParams": [
		{
			"name": "category_id",
			"type": "Number",
			"description": "类目id,没有的时候传-1",
			"required": true
		},
		{
			"name": "extra",
			"type": "Extra",
			"description": "扩展信息",
			"subParams": [
				{
					"name": "platform",
					"type": "String",
					"description": "商品所在平台"
				},
				{
					"name": "product_id",
					"type": "String",
					"description": "商品id"
				},
				{
					"name": "sub_identifier",
					"type": "String",
					"description": "店铺id"
				},
				{
					"name": "sub_identify_type",
					"type": "String",
					"description": "店铺所在平台"
				}
			]
		},
		{
			"name": "identifier",
			"type": "String",
			"description": "erp用户id",
			"required": true
		},
		{
			"name": "identifier_type",
			"type": "String",
			"description": "调用来源(erp名称)",
			"required": true
		},
		{
			"name": "language",
			"type": "String",
			"description": "语种",
			"required": true
		},
		{
			"name": "platform",
			"type": "String",
			"description": "商品所在平台（ae/icbu）",
			"required": true
		},
		{
			"name": "title",
			"type": "String",
			"description": "标题",
			"required": true
		}
	],
	"responseParams": [
		{
			"name": "all_uppercase_words",
			"type": "String []",
			"description": "全大写的单词"
		},
		{
			"name": "contain_core_classes",
			"type": "Boolean",
			"description": "是否包含类目主词"
		},
		{
			"name": "disable_word_list",
			"type": "String []",
			"description": "违禁词"
		},
		{
			"name": "duplicate_word_list",
			"type": "String []",
			"description": "重复的词"
		},
		{
			"name": "language_quality_score",
			"type": "String",
			"description": "语言质量分(0-5)"
		},
		{
			"name": "no_first_uppercase_word_list",
			"type": "String []",
			"description": "首字母未大写单词"
		},
		{
			"name": "over_length_limit",
			"type": "Boolean",
			"description": "标题是否超过长度限制"
		},
		{
			"name": "total_score",
			"type": "Number",
			"description": "总分(0-100)"
		},
		{
			"name": "spell_error_word_list",
			"type": "String []",
			"description": "拼写错误的单词"
		}
	]
}