{
	"name": "taobao.feedflow.item.item.page",
	"apiChineseName": "信息流查看商品列表",
	"description": "信息流查看商品列表",
	"requestParams": [
		{
			"name": "item_query",
			"type": "ItemQueryDto",
			"description": "查询条件",
			"subParams": [
				{
					"name": "campaign_id",
					"type": "Number",
					"description": "计划id",
					"required": true
				},
				{
					"name": "title",
					"type": "String",
					"description": "商品标题"
				},
				{
					"name": "page_size",
					"type": "Number",
					"description": "分页页码，不得超过20"
				},
				{
					"name": "item_id_list",
					"type": "Number[]",
					"description": "商品id列表"
				},
				{
					"name": "current_page",
					"type": "Number",
					"description": "当前页数"
				}
			]
		}
	],
	"responseParams": [
		{
			"name": "result",
			"type": "ResultDto",
			"description": "返回结果对象",
			"subParams": [
				{
					"name": "message",
					"type": "String",
					"description": "返回信息",
					"required": true
				},
				{
					"name": "total_count",
					"type": "Number",
					"description": "商品总数",
					"required": true
				},
				{
					"name": "success",
					"type": "Boolean",
					"description": "调用是否成功,true-成功，false-失败",
