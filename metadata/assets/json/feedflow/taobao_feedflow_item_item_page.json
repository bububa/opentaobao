				},
				{
					"name": "item_list",
					"type": "ItemDto[]",
					"description": "商品列表",
					"required": true,
					"subParams": [
						{
							"name": "item_id",
							"type": "Number",
							"description": "商品id",
							"required": true
						},
						{
							"name": "title",
							"type": "String",
							"description": "商品标题",
							"required": true
						},
						{
							"name": "link_url",
							"type": "String",
							"description": "商品落地页",
							"required": true
