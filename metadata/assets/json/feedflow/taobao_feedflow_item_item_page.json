						{
							"name": "img_url",
							"type": "String",
							"description": "商品主图",
							"required": true
						},
						{
							"name": "is_access_allowed",
							"type": "Boolean",
							"description": "是否可以使用，false不可以进行广告投放",
							"required": true
						},
						{
							"name": "access_allowed_info",
							"type": "AccessAllowedDto",
							"description": "不可以使用的原因",
							"required": true,
							"subParams": [
								{
									"name": "is_access_allowed",
									"type": "Boolean",
									"description": "是否可以使用，false不可以进行广告投放",
									"required": true
								},
								{
									"name": "reason",
									"type": "String",
									"description": "不可以使用的原因",
									"required": true
								}
							]
						}
					]
				}
			]
		}
	]
