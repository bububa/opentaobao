		{
			"name": "auth_param",
			"type": "TopAuthReqDto",
			"description": "授权参数",
			"subParams": [
				{
					"name": "response_type",
					"type": "String",
					"description": "二维码授权 只支持qrcode"
				},
				{
					"name": "client_id",
					"type": "String",
