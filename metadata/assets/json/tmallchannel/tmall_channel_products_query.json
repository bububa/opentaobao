								{
									"name": "sku_id",
									"type": "Number",
									"description": "skuId",
									"required": true
								},
								{
									"name": "sku_number",
									"type": "String",
									"description": "sku商家编码",
									"required": true
								},
								{
									"name": "sku_sc_item_id",
									"type": "Number",
									"description": "sku后端货品",
									"required": true
								},
								{
									"name": "standard_price",
									"type": "Number",
									"description": "基准价",
									"required": true
								},
								{
									"name": "bar_code",
									"type": "String",
									"description": "条形码",
									"required": true
								},
								{
									"name": "picture_url",
