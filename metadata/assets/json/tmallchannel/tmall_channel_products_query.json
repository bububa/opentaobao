		{
			"name": "product_number",
			"type": "String",
			"description": "商家产品编码"
		},
		{
			"name": "sku_number",
			"type": "String",
			"description": "商家SKU编码"
		},
		{
			"name": "product_ids",
			"type": "Number[]",
			"description": "产品Id",
