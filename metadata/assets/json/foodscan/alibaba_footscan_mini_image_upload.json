{
	"id": 50512,
	"name": "alibaba.footscan.mini.image.upload",
	"apiChineseName": "商家端图片上传",
	"description": "提供图片上传功能，同时进行图片的检测",
	"requestParams": [
		{
			"name": "token",
			"type": "String",
			"description": "平台分配的token",
			"required": true
		},
		{
			"name": "req_data",
			"type": "CheckParam",
			"description": "请求数据",
			"required": true,
			"subParams": [
				{
					"name": "base_code",
					"type": "String",
					"description": "图片base64编码，注意字符串前缀",
					"required": true
				},
				{
					"name": "gender",
					"type": "Number",
					"description": "1男2女",
					"required": true
				},
				{
					"name": "nick_name",
					"type": "String",
					"description": "淘宝的nickName"
				},
				{
					"name": "request_id",
					"type": "String",
					"description": "前8位是日期，后10位是随机字符串",
					"required": true
				},
				{
					"name": "index",
					"type": "Number",
					"description": "第几次拍照 0 左脚背面 1 左脚内侧 2 左脚外侧 3右脚脚背 4右脚内侧 5右脚外侧",
					"required": true
				},
				{
					"name": "model_type",
					"type": "Number",
					"description": "1左脚 2右脚",
					"required": true
				},
				{
					"name": "user_id",
					"type": "String",
					"description": "用户唯一标识，可以是淘宝用户Id",
					"required": true
				}
			]
		}
	],
	"responseParams": [
		{
			"name": "result",
			"type": "MtopResult",
			"description": "服务出参",
			"subParams": [
				{
					"name": "code",
					"type": "Number",
					"description": "成功",
					"required": true
				},
				{
					"name": "data",
					"type": "Data",
					"description": "返回i数据",
					"required": true,
					"subParams": [
						{
							"name": "url",
							"type": "String",
							"description": "图片URL",
							"required": true
						},
						{
							"name": "file_name",
							"type": "String",
							"description": "图片名称",
							"required": true
						}
					]
				},
				{
					"name": "message",
					"type": "String",
					"description": "成功",
					"required": true
				}
			]
		}
	]
}