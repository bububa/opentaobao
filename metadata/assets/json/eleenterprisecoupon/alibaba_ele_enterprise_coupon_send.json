				{
					"name": "end_date_time",
					"type": "String",
					"description": "券失效日期时间",
					"required": true
				},
				{
					"name": "phone",
					"type": "String",
					"description": "手机号",
					"required": true
				},
				{
					"name": "condition",
					"type": "String",
					"description": "券使用条件",
					"required": true
