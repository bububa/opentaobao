			"name": "phone",
			"type": "String",
			"description": "手机号",
			"required": true
		},
		{
			"name": "channel",
			"type": "String",
			"description": "固定值，申请获取",
			"required": true
		},
		{
			"name": "latitude",
			"type": "String",
			"description": "纬度",
			"required": true
