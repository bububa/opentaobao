							"required": true
						},
						{
							"name": "click",
							"type": "Number",
							"description": "点击",
							"required": true
						},
						{
							"name": "compare",
							"type": "Number",
							"description": "对比买家数",
							"required": true
						},
						{
							"name": "fb",
							"type": "Number",
							"description": "反馈",
							"required": true
						},
						{
							"name": "impression",
							"type": "Number",
							"description": "曝光",
							"required": true
						},
						{
							"name": "keyword_effects",
							"type": "KeywordEffectEntity[]",
							"description": "词来源",
							"required": true,
							"subParams": [
								{
									"name": "keyword",
									"type": "String",
									"description": "词",
									"required": true
