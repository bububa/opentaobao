{
	"id": 38869,
	"name": "alibaba.mydata.self.product.get",
	"apiChineseName": "获取客户产品相关表现数据",
	"description": "获取客户产品相关表现数据",
	"requestParams": [
		{
			"name": "statistics_type",
			"type": "String",
			"description": "统计周期，可以为\"day\", \"week\", \"month\"",
			"required": true
		},
		{
			"name": "stat_date",
			"type": "String",
			"description": "统计日期",
			"required": true
		},
		{
			"name": "product_ids",
			"type": "Number[]",
			"description": "待查询产品id列表",
			"required": true,
			"maxListSize": 20
		}
	],
	"responseParams": [
		{
			"name": "result",
