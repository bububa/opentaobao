{
	"name": "alibaba.campus.acl.insertrole",
	"apiChineseName": "新增角色",
	"description": "新增角色",
	"requestParams": [
		{
			"name": "company_id",
			"type": "Number",
			"description": "公司id,不填统一为SYS_000"
		},
		{
			"name": "system_id",
			"type": "String",
			"description": "系统id",
			"required": true
		},
		{
			"name": "campus_id",
			"type": "Number",
			"description": "园区id",
			"required": true
		},
		{
			"name": "role_desc",
			"type": "String",
			"description": "角色描述"
		},
		{
			"name": "role_name",
			"type": "String",
			"description": "角色名称",
			"required": true
		},
		{
			"name": "role_type",
			"type": "String",
			"description": "默认人员角色.还有device设备角色类型"
		},
		{
			"name": "role_id",
			"type": "String",
			"description": "角色唯一ID,统一ROLE_开头,不填默认生成ROLE_UUID(32位随机数)",
