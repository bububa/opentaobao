					"required": true
				},
				{
					"name": "success",
					"type": "Boolean",
					"description": "是否成功",
					"required": true
				},
				{
					"name": "error_level",
					"type": "String",
					"description": "错误级别",
					"required": true
				},
				{
					"name": "role_id",
					"type": "String",
					"description": "角色id",
					"required": true
