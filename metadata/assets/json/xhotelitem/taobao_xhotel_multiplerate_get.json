{
	"name": "taobao.xhotel.multiplerate.get",
	"apiChineseName": "复杂房价查询接口",
	"description": "查询复杂房价，支持通过入住人数，连住天数，商品信息，房价信息查询",
	"requestParams": [
		{
			"name": "nod",
			"type": "Number",
			"description": "连住天数(范围1~10)"
		},
		{
			"name": "nop",
			"type": "Number",
			"description": "入住人数(范围1~10)"
		},
		{
			"name": "rate_plan_code",
			"type": "String",
			"description": "卖家的房价code"
		},
		{
			"name": "rate_plan_id",
			"type": "Number",
			"description": "废弃，使用rate_plan_code"
		},
		{
			"name": "out_rid",
			"type": "String",
			"description": "卖家的房型code"
		},
		{
			"name": "gid",
			"type": "Number",
			"description": "废弃，使用out_rid"
		},
		{
			"name": "vendor",
			"type": "String",
			"description": "系统商，一般不填写，使用须申请"
		}
	],
	"responseParams": [
		{
			"name": "rates",
			"type": "MultipleRate []",
			"description": "复杂价格返回结果类",
			"subParams": [
				{
					"name": "occupancy",
					"type": "Number",
					"description": "入住人数"
				},
				{
					"name": "lengthofstay",
					"type": "Number",
					"description": "连住天数"
				},
				{
					"name": "gid",
					"type": "Number",
					"description": "酒店商品id"
				},
				{
					"name": "rpid",
					"type": "Number",
					"description": "房价id"
				},
				{
					"name": "name",
					"type": "String",
					"description": "名称"
				},
				{
					"name": "currency_code",
					"type": "Number",
					"description": "币种"
				},
				{
					"name": "created_time",
					"type": "Date",
					"description": "创建时间"
				},
				{
					"name": "modified_time",
					"type": "Date",
					"description": "修改时间"
				},
				{
					"name": "inventory_price",
					"type": "String",
					"description": "价格和库存信息,包括加床价，加人价等信息。date  日期必须为 T---T+90 日内的日期（T为当天），且不能重复price 价格 int类型 取值范围1-99999999 单位为分quota 库存 int 类型 取值范围  0-999（数量库存）  60000(状态库存关) 61000(状态库存开)addPerson 加人价addBed 加床价"
				}
			]
		}
	]
}