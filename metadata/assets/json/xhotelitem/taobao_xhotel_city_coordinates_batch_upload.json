{
	"name": "taobao.xhotel.city.coordinates.batch.upload",
	"apiChineseName": "上传信息计算飞猪国际城市",
	"description": "给供应商提供计算对应飞猪城市的服务，免去城市名称匹配流程，加快对接流程。目前只适用于国际城市，国内+港澳台暂不支持。\n非实时计算接口，每次批量上传不少于1条的数据，后端离线计算，请于30分钟后再下载结果。",
	"requestParams": [
		{
			"name": "coordinate_list",
			"type": "Coordinate[]",
			"description": "经纬度列表",
			"required": true,
			"maxListSize": 1000,
			"subParams": [
				{
					"name": "country",
					"type": "Number",
					"description": "飞猪国家编码",
					"required": true
				},
				{
					"name": "latitude",
					"type": "String",
					"description": "纬度",
					"required": true
				},
				{
					"name": "longitude",
					"type": "String",
					"description": "经度",
					"required": true
				},
				{
					"name": "outer_id",
					"type": "String",
					"description": "外部经纬度标识id，可以是酒店或城市的id",
					"required": true
				}
			]
		}
	],
	"responseParams": [
		{
			"name": "batch_id",
			"type": "Number",
			"description": "上传成功后的批次号"
		}
	]
}