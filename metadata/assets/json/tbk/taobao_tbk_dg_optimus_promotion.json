{
	"id": 52700,
	"name": "taobao.tbk.dg.optimus.promotion",
	"apiChineseName": "淘宝客-推广者-权益物料精选",
	"description": "推广者使用。支持入参推广者对应的“推广位”和官方提供的“权益物料id”，获取指定权益物料。",
	"requestParams": [
		{
			"name": "page_size",
			"type": "Number",
			"description": "页大小，一次请求请限制在10以内",
			"maxValue": 10
		},
		{
			"name": "page_num",
			"type": "Number",
			"description": "第几页，默认：1"
		},
		{
			"name": "adzone_id",
			"type": "Number",
			"description": "mm_xxx_xxx_xxx的第3段数字",
			"required": true
		},
		{
			"name": "promotion_id",
			"type": "Number",
			"description": "官方提供的权益物料Id。有价券-37104、大额店铺券-37116，更多权益物料id敬请期待！",
			"required": true
		}
	],
	"responseParams": [
		{
			"name": "result_list",
			"type": "MapData []",
			"description": "resultList",
			"subParams": [
				{
					"name": "promotion_type",
					"type": "String",
					"description": "权益类型。1 有价券（需要购买的店铺券或单品券） 2 公开券（直接领取的店铺券或单品券） 3 妈妈券（妈妈渠道领取的店铺券或单品券） 4.品类券 （跨店可用券，可与1，2，3叠加）",
					"required": true
				},
				{
					"name": "condition_type",
					"type": "String",
					"description": "优惠门槛类型： 1 满元 2 满件",
					"required": true
				},
				{
					"name": "discount_type",
					"type": "String",
					"description": "优惠类型： 1 减钱 2 打折",
					"required": true
				},
				{
					"name": "total_count",
					"type": "Number",
					"description": "权益信息-总量（权益初始库存量）",
					"required": true
				},
				{
					"name": "remain_count",
					"type": "Number",
					"description": "权益信息-剩余库存（权益剩余库存量）",
					"required": true
				},
				{
					"name": "display_start_time",
					"type": "String",
					"description": "权益信息展示开始时间，精确到毫秒时间戳",
					"required": true
				},
				{
					"name": "display_end_time",
					"type": "String",
					"description": "权益信息展示结束时间，精确到毫秒时间戳",
					"required": true
				},
				{
					"name": "promotion_list",
					"type": "PromotionList[]",
					"description": "权益信息",
					"required": true,
					"subParams": [
						{
							"name": "entry_used_start_time",
							"type": "String",
							"description": "权益开始时间，精确到毫秒时间戳",
							"required": true
						},
						{
							"name": "entry_used_end_time",
							"type": "String",
							"description": "权益结束时间，精确到毫秒时间戳",
							"required": true
						},
						{
							"name": "entry_condition",
							"type": "String",
							"description": "权益起用门槛，满X元可用，券场景为满元，精确到分，如满100元可用",
							"required": true
						},
						{
							"name": "entry_discount",
							"type": "String",
							"description": "权益面额，券场景为减钱，精确到分",
							"required": true
						}
					]
				},
				{
					"name": "promotion_extend",
					"type": "PromotionExtend",
					"description": "权益扩展信息",
					"required": true,
					"subParams": [
						{
							"name": "recommend_item_list",
							"type": "RecommendItemList[]",
							"description": "权益推荐商品",
							"required": true,
							"subParams": [
								{
									"name": "item_id",
									"type": "Number",
									"description": "权益推荐商品id",
									"required": true
								},
								{
									"name": "url",
									"type": "String",
									"description": "商品链接",
									"required": true
								}
							]
						},
						{
							"name": "youjia_coupon_info",
							"type": "Youjiacouponinfo",
							"description": "有价券信息",
							"required": true,
							"subParams": [
								{
									"name": "item_id",
									"type": "String",
									"description": "有价券商品id",
									"required": true
								},
								{
									"name": "url",
									"type": "String",
									"description": "商品链接",
									"required": true
								}
							]
						},
						{
							"name": "promotion_url",
							"type": "String",
							"description": "权益链接",
							"required": true
						}
					]
				},
				{
					"name": "seller_id",
					"type": "String",
					"description": "店铺信息-卖家ID",
					"required": true
				},
				{
					"name": "nick",
					"type": "String",
					"description": "店铺信息-卖家昵称",
					"required": true
				},
				{
					"name": "shop_title",
					"type": "String",
					"description": "店铺信息-店铺名称",
					"required": true
				},
				{
					"name": "shop_picture_url",
					"type": "String",
					"description": "店铺信息-店铺logo",
					"required": true
				}
			]
		}
	]
}