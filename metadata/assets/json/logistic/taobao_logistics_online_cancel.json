{
	"name": "taobao.logistics.online.cancel",
	"apiChineseName": "取消物流订单接口",
	"description": "调此接口取消发货的订单，重新选择物流公司发货。前提是物流公司未揽收货物。对未发货和已经被物流公司揽收的物流订单，是不能取消的。",
	"requestParams": [
		{
			"name": "tid",
			"type": "Number",
			"description": "淘宝交易ID",
			"required": true
		}
	],
	"responseParams": [
		{
			"name": "is_success",
			"type": "Boolean",
			"description": "成功与失败"
		},
		{
			"name": "modify_time",
			"type": "String",
			"description": "修改时间"
		},
		{
			"name": "recreated_order_id",
			"type": "String",
			"description": "重新创建物流订单id"
		}
	]
}