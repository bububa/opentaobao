{
	"name": "taobao.openim.users.update",
	"apiChineseName": "批量更新用户信息",
	"description": "批量更新用户信息",
	"requestParams": [
		{
			"name": "userinfos",
			"type": "Userinfos[]",
			"description": "用户信息列表",
			"required": true,
			"maxListSize": 100,
			"subParams": [
				{
					"name": "nick",
					"type": "String",
					"description": "昵称，最大长度64字节"
				},
				{
					"name": "icon_url",
					"type": "String",
					"description": "头像url，最大长度512字节"
				},
				{
					"name": "email",
					"type": "String",
					"description": "email地址，最大长度128字节"
				},
				{
					"name": "mobile",
					"type": "String",
					"description": "手机号码，最大长度16字节"
				},
				{
					"name": "taobaoid",
					"type": "String",
					"description": "淘宝账号，最大长度64字节"
				},
				{
					"name": "userid",
					"type": "String",
					"description": "im用户名，最大长度64字节",
					"required": true
				},
				{
					"name": "password",
					"type": "String",
					"description": "im密码，最大长度64字节"
				},
				{
					"name": "remark",
					"type": "String",
					"description": "remark，最大长度128字节"
				},
				{
					"name": "extra",
					"type": "String",
					"description": "扩展字段（Json），最大长度4096字节"
				},
				{
					"name": "career",
					"type": "String",
					"description": "职位，最大长度128字节"
				},
				{
					"name": "vip",
					"type": "String",
					"description": "vip（Json），最大长度512字节"
				},
				{
					"name": "address",
					"type": "String",
					"description": "地址，最大长度512"
				},
				{
					"name": "name",
					"type": "String",
					"description": "名字，最大长度64"
				},
				{
					"name": "age",
					"type": "Number",
					"description": "年龄"
				},
				{
					"name": "gender",
					"type": "String",
					"description": "性别。M: 男。 F：女"
				},
				{
					"name": "wechat",
					"type": "String",
					"description": "微信，最大长度64字节"
				},
				{
					"name": "qq",
					"type": "String",
					"description": "qq，最大长度20字节"
				},
				{
					"name": "weibo",
					"type": "String",
					"description": "微博，最大长度256字节"
				}
			]
		}
	],
	"responseParams": [
		{
			"name": "fail_msg",
			"type": "String []",
			"description": "对应每一个失败用户的具体错误信息"
		},
		{
			"name": "uid_fail",
			"type": "String []",
			"description": "失败的uid列表"
		},
		{
			"name": "uid_succ",
			"type": "String []",
			"description": "成功的uid列表"
		}
	]
}