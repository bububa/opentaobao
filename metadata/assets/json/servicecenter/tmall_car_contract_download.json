{
	"name": "tmall.car.contract.download",
	"apiChineseName": "合同下载",
	"description": "目前天猫开新车会在线上签署一份合同，协议，需要一个个在已卖出打开，另存为pdf，人工一个个下载比较麻烦，期望通过接口直接读取pdf；\n因为比较耗时，建议一个个下载，假设并发下载，很可能限流，每天的调用量有限；",
	"requestParams": [
		{
			"name": "order_id",
			"type": "Number",
			"description": "天猫订单号"
		},
		{
			"name": "html",
			"type": "Boolean",
			"description": "是否下载html，true是html，false是pdf， html速度会快一点"
		}
	],
	"responseParams": [
		{
			"name": "result",
			"type": "Result",
			"description": "结果",
			"subParams": [
				{
					"name": "gmt_current_time",
					"type": "Number",
					"description": "当前时间",
					"required": true
				},
				{
					"name": "msg_code",
					"type": "String",
					"description": "错误码",
					"required": true
				},
				{
					"name": "msg_info",
					"type": "String",
					"description": "错误信息",
					"required": true
				},
				{
					"name": "objects",
					"type": "byte[]",
					"description": "返回的合同二进制文件",
					"required": true
				},
				{
					"name": "success",
					"type": "Boolean",
					"description": "成功与否",
					"required": true
				},
				{
					"name": "cost_time",
					"type": "Number",
					"description": "消耗时间",
					"required": true
				}
			]
		}
	]
}