								{
									"name": "ingredients",
									"type": "Ingredients[]",
									"description": "成分",
									"subParams": [
										{
											"name": "unit",
											"type": "String",
											"description": "成分单位"
										},
										{
											"name": "quantity",
											"type": "Number",
											"description": "成分数值"
										},
										{
											"name": "name",
											"type": "String",
											"description": "成分名"
										}
									]
								},
								{
									"name": "spu_id",
									"type": "String",
									"description": "健康标品库的SpuId"
								},
								{
									"name": "unit_of_per_unit",
									"type": "String",
									"description": "最小使用单位数值剂量单位（mg）"
								},
								{
									"name": "quantity_per_unit",
									"type": "Number",
									"description": "最小使用单位数值（例：10）"
								},
								{
									"name": "approval_number",
									"type": "String",
									"description": "国药准字号"
								},
								{
									"name": "type",
									"type": "String",
									"description": "药品类型（WESTERN_PRESCRIPTION_MEDICINE(1,\"处方药-西药\"),     CHINESE_PRESCRIPTION_MEDICINE(2, \"处方药-中成药\"),     WESTERN_OTC(3, \"OTC-西药\"),     CHINESE_OTC(4, \"OTC-中成药\"),     PIECES(5, \"饮片\")）"
								},
								{
									"name": "norm",
									"type": "String",
									"description": "规格详情"
								}
							]
						}
					]
				},
				{
					"name": "diags",
					"type": "Diag[]",
					"description": "诊断信息，启用适应症和禁忌症规则下必传",
