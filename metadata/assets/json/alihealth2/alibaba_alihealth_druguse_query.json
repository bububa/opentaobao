						{
							"name": "code",
							"type": "String",
							"description": "诊断码"
						},
						{
							"name": "name",
							"type": "String",
							"description": "诊断名"
						}
					]
				},
				{
					"name": "tenant_code",
					"type": "String",
					"description": "租户名",
					"required": true
				}
			]
		}
