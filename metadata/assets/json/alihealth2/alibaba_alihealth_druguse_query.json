{
	"id": 56562,
	"name": "alibaba.alihealth.druguse.query",
	"apiChineseName": "合理用药规则查询",
	"description": "查询用户购买的药品命中的风险规则",
	"requestParams": [
		{
			"name": "command",
			"type": "SafeMedicationReqCommand",
			"description": "入参",
			"subParams": [
				{
					"name": "order_id",
					"type": "String",
					"description": "关联订单"
				},
				{
					"name": "patient",
					"type": "Patient",
					"description": "患者，启用特殊人群规则情况下必传",
					"subParams": [
						{
							"name": "gender",
							"type": "Number",
							"description": "用药人性别（0未知1男2女）"
						},
						{
							"name": "weight",
							"type": "Number",
							"description": "用药人体重-单位kg"
						},
						{
							"name": "user_id",
							"type": "String",
							"description": "淘系用户id"
