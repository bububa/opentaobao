								{
									"name": "buy_amount",
									"type": "Number",
									"description": "本次购买量"
								},
								{
									"name": "history_days",
									"type": "Number",
									"description": "统计周期，不传的话则不做周期限购"
								},
								{
									"name": "history_total_amount",
									"type": "Number",
									"description": "历史购买量。历史购买量需要包含本次购买量，不传的话则不做周期限购"
								}
