	"responseParams": [
		{
			"name": "result",
			"type": "ServiceResult",
			"description": "alinkappserver系统返回的通用结果类",
			"subParams": [
				{
					"name": "success",
					"type": "Boolean",
					"description": "成功与否",
					"required": true
				},
				{
					"name": "error_code",
					"type": "String",
					"description": "错误码",
					"required": true
				},
				{
					"name": "error_msg",
					"type": "String",
					"description": "错误信息",
					"required": true
				},
				{
					"name": "datas",
					"type": "BaseRule[]",
					"description": "结果集",
					"required": true,
					"subParams": [
						{
							"name": "level",
							"type": "Number",
							"description": "级别规则",
							"required": true
						},
						{
							"name": "detail",
							"type": "String",
							"description": "规则的详细数据",
							"required": true
						},
						{
							"name": "desc",
							"type": "String",
							"description": "提示文案",
							"required": true
						},
						{
							"name": "type",
							"type": "String",
							"description": "周期限购：CycleLimitRule，单次限购规则：SingleLimitRule，频次规则：FreqDosageRule， 每日剂量规则：DailyDosageRule，单次剂量规则：SingleDosageRule ，用法规则：WayDosageRule ，重复用药规则：RepetitionRule ，特殊年龄规则：AgeSpecialCrowdRule ，人群标签规则：UserLabelSpecialCrowdRule ，性别诊断冲突规则则：GenderSpecialCrowdRule ，适应症规则：IndicationRule ，相互作用规则：InteractionRule ，禁忌症规则：ContraIndicationRule",
							"required": true
						},
						{
							"name": "spu_id",
							"type": "String",
							"description": "spuid",
							"required": true
						},
						{
							"name": "threshold",
							"type": "String",
							"description": "每次剂量上线（命中每日剂量规则+单次剂量规则）",
							"required": true
						},
						{
							"name": "suggested_freq",
							"type": "String",
							"description": "建议的每日使用频次，如2-3（命中频次规则）",
							"required": true
						},
						{
							"name": "suggested_way",
							"type": "String",
							"description": "建议的用法， 如：口服（命中用法规则）",
							"required": true
						},
						{
							"name": "common_name",
							"type": "String",
							"description": "药品通用名",
							"required": true
						},
						{
							"name": "other_common_name",
							"type": "String",
							"description": "药品通用名（重复用药+相互作用规则）",
							"required": true
						},
						{
							"name": "forbidden_age",
							"type": "String",
							"description": "禁用的年龄区间，单位是天，比如0-1000（命中特殊年龄规则）",
							"required": true
						},
						{
							"name": "forbidden_user",
							"type": "String",
							"description": "禁用的人群（命中特殊人群规则）",
							"required": true
						},
						{
							"name": "gender",
							"type": "String",
							"description": "性别（命中性别诊断冲突规则）",
							"required": true
						},
						{
							"name": "diag_code",
							"type": "String",
							"description": "诊断码（命中性别诊断冲突规则）",
							"required": true
						},
						{
							"name": "diags",
							"type": "String",
							"description": "诊断（命中适应症规则）",
							"required": true
						},
						{
							"name": "forbidden_diags",
							"type": "String",
							"description": "跟该药冲突的诊断码， 如N19.x00（（命中禁忌症规则））",
							"required": true
						},
						{
							"name": "limit_threshold",
							"type": "Number",
							"description": "限购上限制（命中周期限购+单次限购规则）",
							"required": true
						}
					]
				}
			]
		}
	]
}