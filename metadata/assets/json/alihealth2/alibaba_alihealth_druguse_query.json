											"description": "使用量单位"
										},
										{
											"name": "type",
											"type": "String",
											"description": "用量类型（SINGLEDAILYDOSE(\"单日剂量\"),     SINGLEDOSE(\"单次剂量\"),     DRUGCOURSE(\"药品疗程\"),     FREQUENCYADMINISTRATION(\"给药频次\")）"
										},
										{
											"name": "value",
											"type": "Number",
											"description": "使用量"
										}
									]
								},
								{
									"name": "way",
									"type": "String",
									"description": "用法"
								}
							]
						},
						{
							"name": "buy_amount",
							"type": "BuyAmount",
							"description": "购买数量，启用限购的规则条件下必传",
