{
	"name": "tmall.nrt.coupontemplate.query",
	"apiChineseName": "券模板查询",
	"description": "新零售场景，商家拉取在新零售工作台设置的券数据",
	"requestParams": [
		{
			"name": "coupon_type_list",
			"type": "Number[]",
			"description": "券列表",
			"required": true,
			"maxListSize": 20
		},
		{
			"name": "current_page",
			"type": "Number",
			"description": "当前页"
		},
		{
			"name": "page_size",
			"type": "Number",
			"description": "每页数据数量"
		},
		{
			"name": "biz_code",
			"type": "String",
			"description": "业务code阿里指定",
			"required": true
		}
	],
	"responseParams": [
		{
			"name": "message",
			"type": "String",
			"description": "错误message"
		},
		{
			"name": "model",
			"type": "PageData",
			"description": "model",
			"subParams": [
				{
					"name": "current_page",
					"type": "Number",
					"description": "当前页",
					"required": true
				},
				{
					"name": "data_list",
					"type": "CouponTemplateDto[]",
					"description": "数据列表",
					"required": true,
					"subParams": [
						{
							"name": "channel",
							"type": "String",
							"description": "渠道",
							"required": true
						},
						{
							"name": "coupon_name",
							"type": "String",
							"description": "券名称",
							"required": true
						},
						{
							"name": "coupon_type",
							"type": "Number",
							"description": "券type",
							"required": true
						},
						{
							"name": "creator",
							"type": "String",
							"description": "创建钉钉ID",
							"required": true
						},
						{
							"name": "discount",
							"type": "Number",
							"description": "面额",
							"required": true
						},
						{
							"name": "id",
							"type": "Number",
							"description": "券模板ID",
							"required": true
						},
						{
							"name": "item_ids",
							"type": "Number[]",
							"description": "圈选商品列表",
							"required": true
						},
						{
							"name": "mall_ids",
							"type": "Number[]",
							"description": "圈选卖场列表",
							"required": true
						},
						{
							"name": "modified_by",
							"type": "String",
							"description": "修改钉钉ID",
							"required": true
						},
						{
							"name": "send_end_time",
							"type": "Date",
							"description": "发放结束时间yyyy-MM-dd HH:mm:ss",
							"required": true
						},
						{
							"name": "send_start_time",
							"type": "Date",
							"description": "发放开始时间yyyy-MM-dd HH:mm:ss",
							"required": true
						},
						{
							"name": "start_fee",
							"type": "Number",
							"description": "门槛",
							"required": true
						},
						{
							"name": "status",
							"type": "Number",
							"description": "状态",
							"required": true
						},
						{
							"name": "store_ids",
							"type": "Number[]",
							"description": "圈选门店列表",
							"required": true
						},
						{
							"name": "use_end_time",
							"type": "Date",
							"description": "使用结束时间:yyyy-MM-dd HH:mm:ss",
							"required": true
						},
						{
							"name": "use_start_time",
							"type": "Date",
							"description": "使用开始时间:yyyy-MM-dd HH:mm:ss",
							"required": true
						},
						{
							"name": "use_time",
							"type": "Number",
							"description": "领取后可以使用的相对时间，秒或天为单位",
							"required": true
						},
						{
							"name": "use_time_type",
							"type": "Number",
							"description": "使用时间类型",
							"required": true
						},
						{
							"name": "uuid",
							"type": "String",
							"description": "发券code",
							"required": true
						},
						{
							"name": "version",
							"type": "Number",
							"description": "版本号",
							"required": true
						},
						{
							"name": "seller_id",
							"type": "Number",
							"description": "卖家ID",
							"required": true
						},
						{
							"name": "coupon_template_id",
							"type": "Number",
							"description": "券模板ID",
							"required": true
						}
					]
				},
				{
					"name": "page_size",
					"type": "Number",
					"description": "1",
					"required": true
				}
			]
		}
	]
}