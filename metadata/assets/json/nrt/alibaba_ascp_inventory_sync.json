{
	"id": 35425,
	"name": "alibaba.ascp.inventory.sync",
	"apiChineseName": "库存同步接口",
	"description": "新零售联盟商家库存同步接口，用于商家商品库存数量同步到阿里系统",
	"requestParams": [
		{
			"name": "inv_single_item_sync_dto",
			"type": "InvSingleItemSyncDto",
			"description": "库存同步DTO",
			"required": true,
			"subParams": [
				{
					"name": "inv_type",
					"type": "String",
					"description": "库存类型，warehouse-仓库存；store-门店库存，不传默认仓库存"
				},
				{
					"name": "outer_order_no",
					"type": "String",
					"description": "外部操作单号，用于幂等控制",
					"required": true
				},
				{
					"name": "bar_code",
					"type": "String",
					"description": "商品条形码,数字型，作为外部商品唯一外键优先选用该字段，唯一性由商家保证，如果没有请联系阿里小二。bar_code和outer_item_id必须传入其中一个"
				},
				{
					"name": "change_quantity",
					"type": "Number",
					"description": "变化数量，整数表示增加数量，负数表示减少数量",
					"required": true
				},
				{
					"name": "store_code",
					"type": "String",
					"description": "仓编码",
					"required": true
				},
				{
					"name": "biz_code",
					"type": "String",
					"description": "业务码",
					"required": true
				},
				{
					"name": "seller_id",
					"type": "Number",
					"description": "商家id",
					"required": true
				},
				{
					"name": "outer_item_id",
					"type": "String",
					"description": "如果商家没有商品条形码,用该字段，唯一性由商家保证。"
				}
			]
		}
	],
	"responseParams": [
		{
			"name": "result",
			"type": "ResultDO",
			"description": "result",
			"subParams": [
				{
					"name": "succ",
					"type": "Boolean",
					"description": "succ",
					"required": true
				},
				{
					"name": "code",
					"type": "String",
					"description": "code",
					"required": true
				},
				{
					"name": "msg",
					"type": "String",
					"description": "msg",
					"required": true
				}
			]
		}
	]
}