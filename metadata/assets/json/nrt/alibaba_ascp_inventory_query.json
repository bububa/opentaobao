{
	"name": "alibaba.ascp.inventory.query",
	"apiChineseName": "商品库存查询接口",
	"description": "新零售联盟商家库存查询接口，用于商家商品库存数量感知查询",
	"requestParams": [
		{
			"name": "inv_single_item_sync_dto",
			"type": "InvSingleItemSyncDto",
			"description": "系统自动生成",
			"required": true,
			"subParams": [
				{
					"name": "bar_code",
					"type": "String",
					"description": "商品条形码,数字型，作为外部商品唯一外键优先选用该字段，唯一性由商家保证，如果没有请联系阿里小二。bar_code和outer_item_id必须传入其中一个"
				},
				{
					"name": "store_code",
					"type": "String",
					"description": "仓编码",
					"required": true
				},
				{
					"name": "biz_code",
					"type": "String",
					"description": "业务码",
					"required": true
				},
				{
					"name": "seller_id",
					"type": "Number",
					"description": "商家id",
					"required": true
				},
				{
					"name": "outer_item_id",
					"type": "String",
					"description": "如果商家没有商品条形码,用该字段，唯一性由商家保证。"
				}
			]
		}
	],
	"responseParams": [
		{
			"name": "result",
			"type": "ResultDO",
			"description": "result",
			"subParams": [
				{
					"name": "succ",
					"type": "Boolean",
					"description": "succ",
					"required": true
				},
				{
					"name": "data",
					"type": "InvSingleItemSyncDto",
					"description": "data",
					"required": true,
					"subParams": [
						{
							"name": "sellable_quantity",
							"type": "Number",
							"description": "可用销售库存",
							"required": true
						},
						{
							"name": "bar_code",
							"type": "String",
							"description": "商品条形码",
							"required": true
						},
						{
							"name": "store_code",
							"type": "String",
							"description": "仓编码",
							"required": true
						},
						{
							"name": "outer_item_id",
							"type": "String",
							"description": "outerItemId",
							"required": true
						}
					]
				},
				{
					"name": "code",
					"type": "String",
					"description": "错误码",
					"required": true
				},
				{
					"name": "msg",
					"type": "String",
					"description": "错误信息",
					"required": true
				}
			]
		}
	]
}