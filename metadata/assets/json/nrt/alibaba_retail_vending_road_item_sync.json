{
	"name": "alibaba.retail.vending.road.item.sync",
	"apiChineseName": "贩卖机库存商品同步",
	"description": "贩卖机库存商品同步",
	"requestParams": [
		{
			"name": "road_item_sync",
			"type": "RoadItemSyncDto",
			"description": "入参",
			"subParams": [
				{
					"name": "list",
					"type": "RoadItemSaveDto[]",
					"description": "数据",
					"subParams": [
						{
							"name": "road_ids",
							"type": "String",
							"description": "货道编号，若无跟商品编码保持一致",
							"required": true
						},
						{
							"name": "available",
							"type": "Boolean",
							"description": "true 可售 false 不可售",
							"required": true
						},
						{
							"name": "inventory",
							"type": "Number",
							"description": "库存数，全量",
							"required": true
						},
						{
							"name": "volume",
							"type": "Number",
							"description": "容量"
						},
						{
							"name": "item_id",
							"type": "Number",
							"description": "商品编码",
							"required": true
						},
						{
							"name": "barcode",
							"type": "String",
							"description": "条码"
						}
					]
				},
				{
					"name": "device_sn",
					"type": "String",
					"description": "设备Sn",
					"required": true
				}
			]
		}
	],
	"responseParams": [
		{
			"name": "succ",
			"type": "Boolean",
			"description": "成功标识"
		},
		{
			"name": "data",
			"type": "Boolean",
			"description": "是否成功"
		},
		{
			"name": "e_code",
			"type": "String",
			"description": "错误码"
		},
		{
			"name": "e_msg",
			"type": "String",
			"description": "错误消息"
		}
	]
}