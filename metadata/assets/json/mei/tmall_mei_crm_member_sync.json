{
	"name": "tmall.mei.crm.member.sync",
	"apiChineseName": "同步推送会员信息",
	"description": "品牌方通过该api主动推送会员信息。使用场景包括\n1.用户在线上注册后，线下补充信息后，同步到线上。\n2.其他情况的主动推送变更。",
	"requestParams": [
		{
			"name": "mobile",
			"type": "String",
			"description": "会员手机号码"
		},
		{
			"name": "point",
			"type": "Number",
			"description": "会员积分"
		},
		{
			"name": "level",
			"type": "Number",
			"description": "会员等级"
		},
		{
			"name": "extend",
			"type": "String",
			"description": "会员拓展信息"
		},
		{
			"name": "version",
			"type": "Number",
			"description": "该次同步的版本信息（建议使用时间戳）"
		},
		{
			"name": "mix_nick",
			"type": "String",
			"description": "混淆昵称"
		},
		{
			"name": "nick",
			"type": "String",
			"description": "昵称"
		}
	],
	"responseParams": [
		{
			"name": "mei_extra_info",
			"type": "Json",
			"description": "处理的其他信息"
		}
	]
}