{
	"name": "taobao.jst.interactive.assets.query",
	"apiChineseName": "查询可配置任务素材接口",
	"description": "查询可配置任务素材列表，用以配置任务素材",
	"requestParams": [
		{
			"name": "mini_app_id",
			"type": "String",
			"description": "小程序id",
			"required": true
		}
	],
	"responseParams": [
		{
			"name": "assets_list",
			"type": "AssetsConfig []",
			"description": "可配置任务素材列表",
			"subParams": [
				{
					"name": "shop_comp_title",
					"type": "String",
					"description": "任务浮标标题，浏览商品为【浏览10秒得奖励】，观看直播为【观看30秒得奖励】，暂不支持自定义",
					"required": true
				},
				{
					"name": "icon",
					"type": "String",
					"description": "任务图标，80*80",
					"required": true
				},
				{
					"name": "video_id",
					"type": "String",
					"description": "直播间id，配置直播任务时必填",
					"required": true
				},
				{
					"name": "init_btn",
					"type": "String",
					"description": "领取任务按钮",
					"required": true
				},
				{
					"name": "title",
					"type": "String",
					"description": "任务标题",
					"required": true
				},
				{
					"name": "award_btn",
					"type": "String",
					"description": "待领奖按钮",
					"required": true
				},
				{
					"name": "duration",
					"type": "String",
					"description": "任务浏览时长，商品详情页10秒，直播间30秒，暂不支持自定义",
					"required": true
				},
				{
					"name": "accept_btn",
					"type": "String",
					"description": "做任务按钮",
					"required": true
				},
				{
					"name": "item_id",
					"type": "String",
					"description": "商品id，配置商品任务时必填",
					"required": true
				},
				{
					"name": "task_type",
					"type": "Number",
					"description": "任务类型，1=浏览商品，1=观看直播",
					"required": true
				},
				{
					"name": "sub_title",
					"type": "String",
					"description": "任务副标题",
					"required": true
				},
				{
					"name": "task_id",
					"type": "String",
					"description": "任务id，配置任务素材时需要",
					"required": true
				},
				{
					"name": "complete_btn",
					"type": "String",
					"description": "任务完成按钮",
					"required": true
				},
				{
					"name": "desc",
					"type": "String",
					"description": "任务描述",
					"required": true
				}
			]
		}
	]
}