					"description": "取货类型（\"FETCH_HOME\"：上门；\"ON_SHOP\"：到店；\"NONE\"：无需取）",
					"required": true
				},
				{
					"name": "main_biz_order_id",
					"type": "String",
					"description": "主订单号",
					"required": true
				},
				{
					"name": "buyer_id",
					"type": "String",
					"description": "买家标识",
					"required": true
				},
				{
					"name": "buyer_name",
