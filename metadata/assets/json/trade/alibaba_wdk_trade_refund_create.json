						{
							"name": "sub_biz_order_id",
							"type": "String",
							"description": "退货商品子订单号",
							"required": true
						},
						{
							"name": "goods_amount",
							"type": "String",
							"description": "退货件数",
							"required": true
						},
						{
							"name": "fulfill_amount",
							"type": "String",
							"description": "取货件数",
							"required": true
						},
						{
							"name": "sku_code",
