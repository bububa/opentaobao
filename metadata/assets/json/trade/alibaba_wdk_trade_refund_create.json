							"description": "是否称重",
							"required": true
						},
						{
							"name": "gift",
							"type": "Boolean",
							"description": "是否赠品",
							"required": true
						},
						{
							"name": "left_warehouse",
							"type": "Boolean",
							"description": "是否离开货架"
