					"name": "out_counter_id",
					"type": "String",
					"description": "外部专柜Id"
				},
				{
					"name": "status",
					"type": "String[]",
					"description": "订单状态：UNPAID(未支付),PAID(已支付),PARTDISTRIBUTION(部分发货),ALLDISTRIBUTION(全部发货),CANCEL(取消)"
				},
				{
					"name": "order_numbers",
					"type": "String[]",
					"description": "订单集合"
				},
				{
					"name": "end_date",
					"type": "Date",
