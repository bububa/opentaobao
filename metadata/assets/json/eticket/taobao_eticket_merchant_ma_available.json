{
	"name": "taobao.eticket.merchant.ma.available",
	"apiChineseName": "电子凭证核销前校验接口",
	"description": "商家验码之前的调用接口，用来判断是否可以进行核销操作",
	"requestParams": [
		{
			"name": "biz_type",
			"type": "Number",
			"description": "业务类型"
		},
		{
			"name": "code",
			"type": "String",
			"description": "需要被核销的码",
			"required": true
		},
		{
			"name": "consume_num",
			"type": "Number",
			"description": "核销份数",
			"required": true
		},
		{
			"name": "outer_id",
			"type": "String",
			"description": "业务id（订单号）",
			"required": true
		},
		{
			"name": "pos_id",
			"type": "String",
			"description": "机具编号"
		},
		{
			"name": "serial_num",
			"type": "String",
			"description": "核销序列号，需要保证唯一"
		},
		{
			"name": "token",
			"type": "String",
			"description": "需要跟发码通知获取到的参数一致",
			"required": true
		}
	],
	"responseParams": [
		{
			"name": "resp_body",
			"type": "ConsumeMaCallbackResp",
			"description": "回复结果",
			"subParams": [
				{
					"name": "attribute_map",
					"type": "Json",
					"description": "业务回复KV",
					"required": true
				}
			]
		},
		{
			"name": "ret_code",
			"type": "String",
			"description": "子结果码"
		},
		{
			"name": "ret_msg",
			"type": "String",
			"description": "子结果信息"
		}
	]
}