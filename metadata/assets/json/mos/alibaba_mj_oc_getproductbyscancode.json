{
	"name": "alibaba.mj.oc.getproductbyscancode",
	"apiChineseName": "POS商品查询接口",
	"description": "此API用于在银泰商场中，POS端扫码获取商品信息",
	"requestParams": [
		{
			"name": "code",
			"type": "String",
			"description": "码, 对应的信息可能是款号，也有可能是具体的某一个商品",
			"required": true,
			"maxLength": 200
		},
		{
			"name": "code_type",
			"type": "String",
			"description": "条码/二维码/rfid(电子标签),货号、条码、零售+唯一码;ARTNO、BARCODE、UNIQUECODE",
			"required": true
		},
		{
			"name": "shop_code",
			"type": "String",
			"description": "专柜编码",
			"required": true
		},
		{
			"name": "store_code",
			"type": "String",
			"description": "门店编码",
			"required": true
		}
	],
	"responseParams": [
		{
			"name": "product_list",
			"type": "ScanProduct []",
			"description": "data",
			"subParams": [
				{
					"name": "art_no",
					"type": "String",
					"description": "货号",
					"required": true
				},
				{
					"name": "bar_code",
					"type": "String",
					"description": "条码",
					"required": true
				},
				{
					"name": "group_shop_code",
					"type": "String",
					"description": "集团专柜编码",
					"required": true
				},
				{
					"name": "intime_codes",
					"type": "String",
					"description": "收银编码",
					"required": true
				},
				{
					"name": "name",
					"type": "String",
					"description": "商品名称",
					"required": true
				},
				{
					"name": "price",
					"type": "String",
					"description": "销售价",
					"required": true
				},
				{
					"name": "sale_propertys",
					"type": "SaleProperty[]",
					"description": "销售属性",
					"required": true,
					"subParams": [
						{
							"name": "p_id",
							"type": "String",
							"description": "属性Id",
							"required": true
						},
						{
							"name": "p_name",
							"type": "String",
							"description": "属性名称",
							"required": true
						},
						{
							"name": "v_id",
							"type": "String",
							"description": "属性值Id",
							"required": true
						},
						{
							"name": "v_name",
							"type": "String",
							"description": "属性值名称",
							"required": true
						}
					]
				},
				{
					"name": "shop_code",
					"type": "String",
					"description": "专柜Code",
					"required": true
				},
				{
					"name": "sku_id",
					"type": "String",
					"description": "商品Id",
					"required": true
				},
				{
					"name": "source_type",
					"type": "String",
					"description": "商品来源",
					"required": true
				},
				{
					"name": "store_code",
					"type": "String",
					"description": "商品来源",
					"required": true
				},
				{
					"name": "tag_price",
					"type": "String",
					"description": "吊牌价",
					"required": true
				},
				{
					"name": "default_intime_code",
					"type": "String",
					"description": "默认收银编码",
					"required": true
				},
				{
					"name": "unique_code",
					"type": "String",
					"description": "唯一码",
					"required": true
				},
				{
					"name": "tag",
					"type": "String",
					"description": "商品标签",
					"required": true
				}
			]
		}
	]
}