						{
							"name": "coupon_code",
							"type": "String",
							"description": "券码"
						},
						{
							"name": "goods_line_no",
							"type": "Number",
							"description": "商品行号，必须"
						},
						{
							"name": "payment_line_no",
							"type": "Number",
							"description": "支付行号,必须"
						},
						{
							"name": "split_amount",
							"type": "Number",
							"description": "分摊金额，必须"
						},
						{
							"name": "settle_code",
							"type": "String",
							"description": "结算码"
						},
						{
							"name": "pay_time",
							"type": "String",
							"description": "该支付方式支付时间"
						},
						{
							"name": "payment",
							"type": "String",
							"description": "大支付方式"
						},
						{
							"name": "sub_payment",
							"type": "String",
							"description": "小支付方式"
						},
						{
							"name": "pay_type",
							"type": "Number",
							"description": "1:支付，2:优惠"
						},
						{
							"name": "extend_params",
							"type": "String",
							"description": "是否压卡"
						}
					]
				},
				{
					"name": "extend_params",
					"type": "String",
					"description": "扩展信息。"
				}
			]
		}
	],
	"responseParams": [
		{
			"name": "ex_code",
			"type": "Number",
			"description": "errCode"
		},
		{
			"name": "ex_msg",
			"type": "String",
			"description": "errMsg"
		},
		{
			"name": "is_success",
			"type": "Boolean",
			"description": "success"
		},
		{
			"name": "out_trade_no",
			"type": "String",
			"description": "POS交易号"
		}
	]
}