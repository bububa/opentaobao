{
	"name": "alipay.baoxian.claim.uploadattachment",
	"apiChineseName": "资料上传接口",
	"description": "给合作伙伴上传申请理赔材料",
	"requestParams": [
		{
			"name": "out_biz_no",
			"type": "String",
			"description": "外部业务号，唯一",
			"required": true
		},
		{
			"name": "biz_source",
			"type": "String",
			"description": "业务来源"
		},
		{
			"name": "sp_no",
			"type": "String",
			"description": "标准产品ID"
		},
		{
			"name": "attachment_key",
			"type": "String",
			"description": "文件名,必须带后缀名。例如：test.png,test.doc,test.pdf",
			"required": true
		},
		{
			"name": "attachment_byte",
			"type": "byte[]",
			"description": "文件字节数组",
			"required": true
		},
		{
			"name": "base64_bytes",
			"type": "Boolean",
			"description": "是否base格式的字节数组"
		},
		{
			"name": "policy_biz_no",
			"type": "String",
			"description": "保单外部业务单号"
		},
		{
			"name": "upload_user",
			"type": "String",
			"description": "上传者用户标识",
			"required": true
		}
	],
	"responseParams": [
		{
			"name": "upload_result",
			"type": "UploadResult",
			"description": "result",
			"subParams": [
				{
					"name": "model",
					"type": "InsAttachmentUploadVo",
					"description": "model",
					"required": true,
					"subParams": [
						{
							"name": "oss_path",
							"type": "String",
							"description": "ossPath",
							"required": true
						},
						{
							"name": "e_tag",
							"type": "String",
							"description": "eTag",
							"required": true
						},
						{
							"name": "size",
							"type": "Number",
							"description": "size",
							"required": true
						}
					]
				},
				{
					"name": "error_code",
					"type": "String",
					"description": "errorCode",
					"required": true
				},
				{
					"name": "error_msg",
					"type": "String",
					"description": "errorMsg",
					"required": true
				},
				{
					"name": "is_success",
					"type": "Boolean",
					"description": "isSuccess",
					"required": true
				}
			]
		}
	]
}