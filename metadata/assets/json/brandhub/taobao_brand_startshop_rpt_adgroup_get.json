{
	"name": "taobao.brand.startshop.rpt.adgroup.get",
	"apiChineseName": "明星店铺推广单元报表数据查询",
	"description": "获取明星店铺广告adgroup分日报表数据，只能查询近90天内的数据，包括展现量，点击量等",
	"requestParams": [
		{
			"name": "traffic_type",
			"type": "String",
			"description": "流量类型 1: PC站内, 2: PC站外 , 4: 无线站内, 5: 无线站外,支持多种一起查询,如1,2,4,5",
			"required": true
		},
		{
			"name": "effect",
			"type": "Number",
			"description": "转化周期默认15天,3,7,15"
		},
		{
			"name": "page_index",
			"type": "String",
			"description": "当前页数"
		},
		{
			"name": "page_size",
			"type": "String",
			"description": "每页条数"
		},
		{
			"name": "start_date",
			"type": "String",
			"description": "开始时间(最多可查询最近90天)",
			"required": true
		},
		{
			"name": "end_date",
			"type": "String",
			"description": "截至时间(最晚到昨天)",
			"required": true
		}
	],
	"responseParams": [
		{
			"name": "error_param",
			"type": "String",
			"description": "错误信息"
		},
