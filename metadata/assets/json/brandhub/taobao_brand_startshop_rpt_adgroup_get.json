				{
					"name": "thedate",
					"type": "String",
					"description": "日期",
					"required": true
				},
				{
					"name": "impression",
					"type": "Number",
					"description": "展现量",
					"required": true
				},
				{
					"name": "click",
					"type": "Number",
					"description": "点击量",
					"required": true
				},
				{
					"name": "ctr",
					"type": "String",
					"description": "点击率",
					"required": true
				},
				{
