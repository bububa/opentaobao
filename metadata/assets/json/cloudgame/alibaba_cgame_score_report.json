{
	"name": "alibaba.cgame.score.report",
	"apiChineseName": "云游戏战绩上传通用接口",
	"description": "阿里云游戏, CP游戏合作方通用游戏结果回传接口",
	"requestParams": [
		{
			"name": "report_data",
			"type": "CpCallbackReportDto",
			"description": "通用战绩回传数据",
			"subParams": [
				{
					"name": "biz_type",
					"type": "String",
					"description": "云游戏业务类型, 不同业务类型对应bizData格式不同"
				},
				{
					"name": "ticket",
					"type": "String",
					"description": "启动游戏票据,可以用来换取gameSessionId"
				},
				{
					"name": "mix_user_id",
					"type": "String",
					"description": "云游戏加密用户Id"
				},
				{
					"name": "game_app_key",
					"type": "String",
					"description": "约定的appKey"
				},
				{
					"name": "uuid",
					"type": "String",
					"description": "全局唯一id, 用于接口幂等"
				},
				{
					"name": "report_timestamp",
					"type": "Number",
					"description": "时间戳, 毫秒级别"
				},
				{
					"name": "callback_biz_data",
					"type": "String",
					"description": "回传业务数据, 针对biztype=0, 格式包含: 过关数, 剩余生命, 每关使用生命,每关耗时."
				}
			]
		}
	],
	"responseParams": [
		{
			"name": "result",
			"type": "Result",
			"description": "接口返回model",
			"subParams": [
				{
					"name": "code",
					"type": "String",
					"description": "结果码",
					"required": true
				},
				{
					"name": "message",
					"type": "String",
					"description": "返回消息",
					"required": true
				},
				{
					"name": "data",
					"type": "String",
					"description": "结果消息",
					"required": true
				}
			]
		}
	]
}