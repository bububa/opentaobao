{
	"name": "taobao.qimen.inventory.report",
	"apiChineseName": "库存盘点通知接口",
	"description": "WMS调用奇门的接口,将库存盘点情况回传ERP",
	"requestParams": [
		{
			"name": "request",
			"type": "InventoryReportRequest",
			"subParams": [
				{
					"name": "totalPage",
					"type": "Number",
					"description": "总页数",
					"required": true
				},
				{
					"name": "currentPage",
					"type": "Number",
					"description": "当前页(从1开始)",
					"required": true
				},
				{
					"name": "pageSize",
					"type": "Number",
					"description": "每页记录的条数",
					"required": true
				},
				{
					"name": "warehouseCode",
					"type": "String",
					"description": "仓库编码",
					"required": true
				},
				{
					"name": "checkOrderCode",
					"type": "String",
					"description": "盘点单编码",
					"required": true
				},
				{
					"name": "checkOrderId",
					"type": "String",
					"description": "仓储系统的盘点单编码"
				},
				{
					"name": "ownerCode",
					"type": "String",
					"description": "货主编码",
					"required": true
				},
				{
					"name": "checkTime",
					"type": "String",
					"description": "盘点时间(YYYY-MM-DD HH:MM:SS)"
				},
				{
					"name": "outBizCode",
					"type": "String",
					"description": "外部业务编码(消息ID;用于去重;ISV对于同一请求;分配一个唯一性的编码。用来保证因为网络等原因导致重复传输;请求不 会被重复处理)",
					"required": true
				},
				{
					"name": "remark",
					"type": "String",
					"description": "备注"
				},
				{
					"name": "items",
					"type": "Item[]",
					"description": "商品库存信息列表",
					"subParams": [
						{
							"name": "itemCode",
							"type": "String",
							"description": "商品编码",
							"required": true
						},
						{
							"name": "itemId",
							"type": "String",
							"description": "仓储系统商品ID"
						},
						{
							"name": "inventoryType",
							"type": "String",
							"description": "库存类型(ZP=正品;CC=残次;JS=机损;XS= 箱损;ZT=在途库存;默认为ZP)"
						},
						{
							"name": "quantity",
							"type": "Number",
							"description": "盘盈盘亏商品变化量(盘盈为正数;盘亏为负数)",
							"required": true
						},
						{
							"name": "batchCode",
							"type": "String",
							"description": "批次编码"
						},
						{
							"name": "productDate",
							"type": "String",
							"description": "商品生产日期(YYYY-MM-DD)"
						},
						{
							"name": "expireDate",
							"type": "String",
							"description": "商品过期日期(YYYY-MM-DD)"
						},
						{
							"name": "produceCode",
							"type": "String",
							"description": "生产批号"
						},
						{
							"name": "snCode",
							"type": "String",
							"description": "商品序列号"
						},
						{
							"name": "remark",
							"type": "String",
							"description": "备注"
						},
						{
							"name": "totalQty",
							"type": "Number",
							"description": "库存商品总量"
						}
					]
				},
				{
					"name": "extendProps",
					"type": "Map",
					"description": "扩展属性"
				},
				{
					"name": "remark",
					"type": "String",
					"description": "备注"
				},
				{
					"name": "adjustType",
					"type": "String",
					"description": "变动类型：CHECK=盘点 ADJUST=调整",
					"required": true
				}
			]
		}
	],
	"responseParams": [
		{
			"name": "response",
			"type": "Response",
			"subParams": [
				{
					"name": "flag",
					"type": "String",
					"description": "响应结果:success|failure",
					"required": true
				},
				{
					"name": "code",
					"type": "String",
					"description": "响应码",
					"required": true
				},
				{
					"name": "message",
					"type": "String",
					"description": "响应信息",
					"required": true
				}
			]
		}
	]
}