				{
					"name": "oid",
					"type": "Number",
					"description": "酒店订单id"
				},
				{
					"name": "hid",
					"type": "Number",
					"description": "酒店id"
				},
				{
					"name": "rid",
					"type": "Number",
					"description": "房型id"
				},
				{
					"name": "gid",
