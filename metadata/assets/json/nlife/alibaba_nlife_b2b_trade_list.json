{
	"name": "alibaba.nlife.b2b.trade.list",
	"apiChineseName": "获取企业下的采购单列表",
	"description": "获取指定门店下的采购单列表",
	"requestParams": [
		{
			"name": "start_effective_date",
			"type": "Date",
			"description": "采购单生效时间开始范围"
		},
		{
			"name": "end_effective_date",
			"type": "Date",
			"description": "采购单生效时间结束范围"
		},
		{
			"name": "page_no",
			"type": "Number",
			"description": "查询的页码",
			"required": true
		},
		{
			"name": "page_size",
			"type": "Number",
			"description": "每页的数量",
			"required": true
		},
		{
			"name": "ent_id",
			"type": "Number",
			"description": "企业ID",
			"required": true
		}
	],
	"responseParams": [
		{
			"name": "data",
			"type": "ProcurementResponseDo",
			"description": "查询结果数据",
			"subParams": [
				{
					"name": "ent_id",
					"type": "Number",
					"description": "企业ID",
					"required": true
				},
				{
					"name": "total_results",
					"type": "Number",
					"description": "获取到的企业采购单的总数量",
					"required": true
				},
				{
					"name": "trade_list",
					"type": "Tradelist[]",
					"description": "采购订单列表",
					"required": true,
					"subParams": [
						{
							"name": "gmt_effective",
							"type": "Date",
							"description": "采购单生效时间",
							"required": true
						},
						{
							"name": "ent_trade_no",
							"type": "String",
							"description": "企业采购单号",
							"required": true
						},
						{
							"name": "trade_store_list",
							"type": "Tradestorelist[]",
							"description": "门店采购单列表",
							"required": true,
							"subParams": [
								{
									"name": "trade_no",
									"type": "String",
									"description": "门店采购单号",
									"required": true
								},
								{
									"name": "supplier_id",
									"type": "Number",
									"description": "供应商ID",
									"required": true
								},
								{
									"name": "store_id",
									"type": "Number",
									"description": "门店ID",
									"required": true
								}
							]
						}
					]
				}
			]
		}
	]
}