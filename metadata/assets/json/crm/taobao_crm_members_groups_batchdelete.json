			"maxListSize": 20
		},
		{
			"name": "group_ids",
			"type": "Number[]",
			"description": "会员需要删除的分组",
			"required": true,
			"maxListSize": 10,
			"minValue": 1
		}
	],
	"responseParams": [
		{
			"name": "is_success",
			"type": "Boolean",
