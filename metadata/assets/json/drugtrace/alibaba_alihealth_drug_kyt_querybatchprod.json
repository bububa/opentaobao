{
	"name": "alibaba.alihealth.drug.kyt.querybatchprod",
	"apiChineseName": "批次产品查询(根据企业名和批次号查询产品信息)",
	"description": "根据企业名和批次号查询药品信息，支持使用更名之前的老企业名查询，支持批次号大小写模糊，应用于药店或医院入库环节，通过在入库环节获取赋码的产品目录，可强制要求对相应的产品必须进行扫码入库；",
	"requestParams": [
		{
			"name": "product_ent_name",
			"type": "String",
			"description": "生产企业名称；支持新老企业名称匹配",
			"required": true
		},
		{
			"name": "product_batch_no",
			"type": "String",
			"description": "生产批号;支持大小写模糊匹配",
			"required": true
		},
		{
			"name": "bureau_name",
			"type": "String",
			"description": "社保局(所属地市名称)",
			"required": true
		},
		{
			"name": "terminal_name",
			"type": "String",
			"description": "请求终端名称",
			"required": true
		},
		{
			"name": "terminal_type",
			"type": "String",
			"description": "终端类型：1005100-零售，1005200-医疗",
			"required": true
		},
		{
			"name": "invocation",
			"type": "String",
			"description": "调用方式：formal-正式、test-测试",
			"required": true
		}
	],
	"responseParams": [
		{
			"name": "result",
			"type": "Result",
			"description": "接口返回结果",
			"subParams": [
				{
					"name": "success",
					"type": "Boolean",
					"description": "是否成功",
					"required": true
				},
				{
					"name": "models",
					"type": "BatchProductInfoDTO[]",
					"description": "批次产品信息DTO",
					"required": true,
					"subParams": [
						{
							"name": "ent_name",
							"type": "String",
							"description": "生产企业名称",
							"required": true
						},
						{
							"name": "pkg_spec",
							"type": "String",
							"description": "包装规格",
							"required": true
						},
						{
							"name": "product_date",
							"type": "String",
							"description": "生产日期",
							"required": true
						},
						{
							"name": "produce_batch_no",
							"type": "String",
							"description": "生产批号",
							"required": true
						},
						{
							"name": "drug_ent_base_id",
							"type": "String",
							"description": "药品唯一ID",
							"required": true
						},
						{
							"name": "physic_name",
							"type": "String",
							"description": "药品通用名",
							"required": true
						},
						{
							"name": "expire_date",
							"type": "String",
							"description": "失效日期",
							"required": true
						},
						{
							"name": "ref_ent_id",
							"type": "String",
							"description": "生产企业唯一ID",
							"required": true
						},
						{
							"name": "curr_ent_id",
							"type": "String",
							"description": "生产企业流水ID",
							"required": true
						},
						{
							"name": "prepn_type",
							"type": "String",
							"description": "剂型",
							"required": true
						},
						{
							"name": "sdc_code",
							"type": "String",
							"description": "药品本位码",
							"required": true
						},
						{
							"name": "prepn_spec",
							"type": "String",
							"description": "制剂规格",
							"required": true
						},
						{
							"name": "approval_licence_no",
							"type": "String",
							"description": "批准文号",
							"required": true
						}
					]
				},
				{
					"name": "msg_info",
					"type": "String",
					"description": "msgInfo",
					"required": true
				},
				{
					"name": "msg_code",
					"type": "String",
					"description": "msgCode",
					"required": true
				}
			]
		}
	]
}