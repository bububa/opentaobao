		{
			"name": "ref_ent_id",
			"type": "String",
			"description": "企业Id",
			"required": true
		},
		{
			"name": "ent_name",
			"type": "String",
			"description": "企业名称",
			"required": true
		},
		{
			"name": "csr",
			"type": "String",
			"description": "证书签名请求",
			"required": true
