{
	"name": "alibaba.security.jaq.spamregisterprevention.result.fetch",
	"apiChineseName": "获取垃圾注册防控结果",
	"description": "获取垃圾注册防控结果",
	"requestParams": [
		{
			"name": "phone_number",
			"type": "String",
			"description": "账号绑定的手机号码",
			"required": true
		},
		{
			"name": "ip",
			"type": "String",
			"description": "账号绑定的IP地址",
			"required": true
		},
		{
			"name": "context",
			"type": "String",
			"description": "场景描述，String必须在下面可以使用的场景中选择。可以使用的场景为：Login：登录事件；Register：注册事件；Trade:交易事件；Payment:支付事件;Refund：退款事件\t；Loan：借款事件；Transfer：转账事件；Withdraw\t：提现事件；Modify：修改事件；Click：点击事件；Activate：激活事件；\tOther：其他事件。",
			"required": true
		},
		{
			"name": "source",
			"type": "Number",
			"description": "登录来源。可以输入的参数如下：1：PC网页；2：移动网页；3：APP；4：其他",
			"required": true
		},
		{
			"name": "email",
			"type": "String",
			"description": "账号绑定的email地址"
		},
		{
			"name": "user_id",
			"type": "String",
			"description": "账号的全局唯一标识"
		},
		{
			"name": "register_url",
			"type": "String",
			"description": "当前注册的页面URL，Source为1，2时，该参数必选"
		},
		{
			"name": "agent",
			"type": "String",
			"description": "agent，发送HTTP请求的代理"
		},
		{
			"name": "cookie",
			"type": "String",
			"description": "Cookie"
		},
		{
			"name": "session_id",
			"type": "String",
			"description": "Session id"
		},
		{
			"name": "mac_address",
			"type": "String",
			"description": "硬件信息"
		},
		{
			"name": "referer",
			"type": "String",
			"description": "上一跳信息"
		},
		{
			"name": "nick_name",
			"type": "String",
			"description": "账号绑定的呢称"
		},
		{
			"name": "company_name",
			"type": "String",
			"description": "账号绑定的公司名字"
		},
		{
			"name": "address",
			"type": "String",
			"description": "账号绑定的注册的地址"
		},
		{
			"name": "id_number",
			"type": "String",
			"description": "账号绑定的身份证号"
		},
		{
			"name": "bank_card_number",
			"type": "String",
			"description": "账号绑定的银行卡号"
		},
		{
			"name": "js_token",
			"type": "String",
			"description": "接入JS后从服务端获取的token"
		},
		{
			"name": "sdk_token",
			"type": "String",
			"description": "接入SDK后从服务端获取的token"
		}
	],
	"responseParams": [
		{
			"name": "jaq_account_risk_result",
			"type": "JaqAccountRiskResult",
			"description": "账号风控返回结果",
			"subParams": [
				{
					"name": "error_code",
					"type": "Number",
					"description": "错误码"
				},
				{
					"name": "data",
					"type": "JaqAccountRiskData",
					"description": "风控结果的真正数据内容",
					"subParams": [
						{
							"name": "detail_list",
							"type": "JaqAccountRiskDetailItem[]",
							"description": "详情列表",
							"subParams": [
								{
									"name": "id",
									"type": "String",
									"description": "rule id"
								},
								{
									"name": "name",
									"type": "String",
									"description": "rule name"
								},
								{
									"name": "decision",
									"type": "Number",
									"description": "决定，0：可以接受；1：应该拒绝；2：需要人工审查"
								},
								{
									"name": "score",
									"type": "Number",
									"description": "分数"
								}
							]
						},
						{
							"name": "event_id",
							"type": "String",
							"description": "事件id"
						},
						{
							"name": "user_id",
							"type": "String",
							"description": "注册用户id"
						},
						{
							"name": "final_score",
							"type": "Number",
							"description": "最终得分"
						},
						{
							"name": "final_decision",
							"type": "Number",
							"description": "最终决定。0：可以接受；1：应该拒绝；2：需要人工审核"
						}
					]
				},
				{
					"name": "error_msg",
					"type": "String",
					"description": "错误信息"
				}
			]
		}
	]
}