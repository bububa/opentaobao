				},
				{
					"name": "cpc",
					"type": "String",
					"description": "平均点击花费",
					"required": true
				},
				{
					"name": "ctr",
					"type": "String",
					"description": "点击率",
					"required": true
				},
				{
					"name": "cost",
					"type": "String",
					"description": "消耗金额，单位元",
					"required": true
				},
				{
					"name": "click_cnt",
					"type": "String",
					"description": "点击量",
					"required": true
				},
				{
					"name": "impression_cnt",
					"type": "String",
					"description": "曝光量",
					"required": true
				},
				{
					"name": "date",
					"type": "String",
					"description": "日期",
					"required": true
