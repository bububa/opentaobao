{
	"name": "taobao.picture.update",
	"apiChineseName": "修改图片名字",
	"description": "修改指定图片的图片名",
	"requestParams": [
		{
			"name": "picture_id",
			"type": "Number",
			"description": "要更改名字的图片的id",
			"required": true
		},
		{
			"name": "new_name",
			"type": "String",
			"description": "新的图片名，最大长度50字符，不能为空",
			"required": true,
			"maxLength": 50
		}
	],
	"responseParams": [
		{
			"name": "done",
			"type": "Boolean",
			"description": "更新是否成功"
		}
	]
}