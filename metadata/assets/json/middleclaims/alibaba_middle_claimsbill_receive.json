			"subParams": [
				{
					"name": "success",
					"type": "Boolean",
					"description": "系统调用结果",
					"required": true
				},
				{
					"name": "data",
					"type": "Boolean",
					"description": "业务结果",
					"required": true
				},
				{
					"name": "repeated",
					"type": "Boolean",
					"description": "是否重复",
					"required": true
				},
				{
					"name": "retry",
					"type": "Boolean",
