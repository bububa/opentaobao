{
	"name": "taobao.opentrade.group.query",
	"apiChineseName": "组团购场景查询商品组团详情",
	"description": "组团购场景下，查询商品开团详情",
	"requestParams": [
		{
			"name": "item_id",
			"type": "Number",
			"description": "商品id",
			"required": true
		},
		{
			"name": "open_user_id",
			"type": "String",
			"description": "用户openId"
		},
		{
			"name": "order_by",
			"type": "Number",
			"description": "0 返回未成团列表，1 返回已成团列表"
		},
		{
			"name": "page_index",
			"type": "Number",
			"description": "页数"
		},
		{
			"name": "page_size",
			"type": "Number",
			"description": "每页展示条数，不能超过100"
		},
		{
			"name": "group_activity_id",
			"type": "Number",
			"description": "组团活动id"
		},
		{
			"name": "with_expire",
			"type": "Boolean",
			"description": "是否返回已过期的团，true 返回，false 不返回"
		}
	],
	"responseParams": [
		{
			"name": "total_count",
			"type": "Number",
			"description": "总记录数"
		},
		{
			"name": "results",
			"type": "GroupDetailResponse []",
			"description": "团信息",
			"subParams": [
				{
					"name": "joined",
					"type": "Number",
					"description": "已参团人数",
					"required": true
				},
				{
					"name": "goal",
					"type": "Number",
					"description": "成团目标数",
					"required": true
				},
				{
					"name": "group_type",
					"type": "Number",
					"description": "组团类型，0 拼团 1 团购",
					"required": true
				},
				{
					"name": "group_id",
					"type": "Number",
					"description": "团id",
					"required": true
				},
				{
					"name": "item_id",
					"type": "Number",
					"description": "商品id",
					"required": true
				},
				{
					"name": "leader",
					"type": "RopGroupMemberInfo",
					"description": "团长信息",
					"required": true,
					"subParams": [
						{
							"name": "nick",
							"type": "String",
							"description": "昵称",
							"required": true
						},
						{
							"name": "avatar",
							"type": "String",
							"description": "用户头像",
							"required": true
						}
					]
				},
				{
					"name": "members",
					"type": "RopGroupMemberInfo[]",
					"description": "团员信息",
					"required": true,
					"subParams": [
						{
							"name": "nick",
							"type": "String",
							"description": "昵称",
							"required": true
						},
						{
							"name": "avatar",
							"type": "String",
							"description": "头像",
							"required": true
						}
					]
				},
				{
					"name": "done",
					"type": "Boolean",
					"description": "是否成团",
					"required": true
				},
				{
					"name": "expiration",
					"type": "Date",
					"description": "成团有效截止日期",
					"required": true
				},
				{
					"name": "payed",
					"type": "Number",
					"description": "下单付款人数",
					"required": true
				},
				{
					"name": "member_pay_status",
					"type": "Number",
					"description": "1-未付款，2-已付款，4-已退款，6-交易成功，8-交易关闭",
					"required": true
				},
				{
					"name": "group_activity_id",
					"type": "Number",
					"description": "组团活动id",
					"required": true
				}
			]
		}
	]
}