{
	"name": "taobao.opentrade.create.order",
	"apiChineseName": "订单创建",
	"description": "交易开放创建订单",
	"requestParams": [
		{
			"name": "out_id",
			"type": "String",
			"description": "外部订单ID",
			"required": true
		},
		{
			"name": "open_user_id",
			"type": "String",
			"description": "买家openID",
			"required": true
		},
		{
			"name": "full_name",
			"type": "String",
			"description": "收货地址的收件人姓名",
			"required": true
		},
		{
			"name": "mobile",
			"type": "String",
			"description": "收货地址的手机号码",
			"required": true
		},
		{
			"name": "address",
			"type": "String",
			"description": "收货地址",
			"required": true
		},
		{
			"name": "seller_memo",
			"type": "String",
			"description": "卖家备忘"
		},
		{
			"name": "buyer_memo",
			"type": "String",
			"description": "卖家备忘"
		},
		{
			"name": "item_infos",
			"type": "ItemInfo[]",
			"description": "商品信息，一次不能超过10个",
			"required": true,
			"maxListSize": 20,
			"subParams": [
				{
					"name": "item_id",
					"type": "Number",
					"description": "商品ID",
					"required": true
				},
				{
					"name": "sku_id",
					"type": "Number",
					"description": "商品SKU"
				},
				{
					"name": "quantity",
					"type": "Number",
					"description": "商品数量",
					"required": true
				},
				{
					"name": "title",
					"type": "String",
					"description": "订单页显示的商品名称"
				},
				{
					"name": "image_url",
					"type": "String",
					"description": "订单页显示的商品主图地址，只能为图片空间中的图片。只需传入图片地址的path，无需域名"
				}
			]
		}
	],
	"responseParams": [
		{
			"name": "order_id",
			"type": "String",
			"description": "订单ID"
		}
	]
}