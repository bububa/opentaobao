{
	"name": "alibaba.einvoice.income.device.return",
	"apiChineseName": "服务商回传客户端设备列表",
	"description": "服务商回传客户端agent所处环境的设备列表，比如扫描仪",
	"requestParams": [
		{
			"name": "device_list",
			"type": "String[]",
			"description": "设备列表，success=true时必填",
			"maxListSize": 20
		},
		{
			"name": "error_code",
			"type": "String",
			"description": "错误码，success=false时必填"
		},
		{
			"name": "error_message",
			"type": "String",
			"description": "错误信息，success=false时必填"
		},
		{
			"name": "req_index",
			"type": "String",
			"description": "请求标识",
			"required": true
		},
		{
			"name": "success",
			"type": "Boolean",
			"description": "查询设备是否成功，true=成功，false=失败",
			"required": true
		}
	],
	"responseParams": [
		{
			"name": "is_success",
			"type": "Boolean",
			"description": "接口是否调用成功"
		}
	]
}