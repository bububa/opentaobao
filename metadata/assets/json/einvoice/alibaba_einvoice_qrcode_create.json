{
	"id": 31273,
	"name": "alibaba.einvoice.qrcode.create",
	"apiChineseName": "扫码开票二维码生成",
	"description": "扫码开票功能中的二维码生成接口，pos机等发起请求生成二维码",
	"requestParams": [
		{
			"name": "invoice_items",
			"type": "BillItemDo[]",
			"description": "发票商品明细",
			"required": true,
			"maxListSize": 20,
			"subParams": [
				{
					"name": "quantity",
					"type": "String",
					"description": "商品数量"
				},
				{
					"name": "amount",
					"type": "String",
					"description": "商品价税合计",
					"required": true
				},
				{
					"name": "specification",
					"type": "String",
					"description": "商品规格型号"
				},
				{
					"name": "unit",
					"type": "String",
					"description": "商品单位"
				},
				{
					"name": "item_name",
					"type": "String",
					"description": "商品名称",
					"required": true
				}
			]
		},
		{
			"name": "payee_register_no",
			"type": "String",
			"description": "收款方税务登记证号",
			"required": true
		},
		{
			"name": "order_id",
			"type": "String",
			"description": "开票的订单号，同结算单订单号",
			"required": true
		},
		{
			"name": "sum_price",
			"type": "String",
			"description": "开票金额",
			"required": true
		},
		{
			"name": "source_id",
			"type": "String",
			"description": "请求方唯一标识ID，例如POS机编码",
			"required": true
		},
		{
			"name": "qr_logo",
			"type": "String",
			"description": "二维码图片中间的logo"
		},
		{
			"name": "width",
			"type": "Number",
			"description": "二维码图片宽度，默认=450"
		},
		{
			"name": "height",
			"type": "Number",
			"description": "二维码图片高度，默认=450"
		},
		{
			"name": "qr_type",
			"type": "Number",
			"description": "二维码返回类型：1=二维码背后的URL，2=二维码图片CDN URL，3=二维码二进制数据流",
			"required": true
		},
		{
			"name": "platform",
			"type": "String",
			"description": "请求方ERP系统平台，同结算单同步的platform，不填默认=platform"
		}
	],
	"responseParams": [
		{
			"name": "result",
			"type": "ResultSet",
			"description": "result",
			"subParams": [
				{
					"name": "error_message",
					"type": "String",
					"description": "errorMessage",
					"required": true
				},
				{
					"name": "result",
					"type": "String",
					"description": "result",
					"required": true
				},
				{
					"name": "error_code",
					"type": "String",
					"description": "errorCode",
					"required": true
				}
			]
		}
	]
}