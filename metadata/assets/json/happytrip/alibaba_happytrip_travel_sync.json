{
	"id": 38152,
	"name": "alibaba.happytrip.travel.sync",
	"apiChineseName": "差旅申请单同步接口",
	"description": "以外部差旅申请单id（outer_travel_head_id）为主键，保存或更新差旅单信息到欢行系统中",
	"requestParams": [
		{
			"name": "travel_head_dto",
			"type": "TravelHeadDto",
			"description": "差旅申请单对象",
			"required": true,
			"subParams": [
				{
					"name": "create_user_id",
					"type": "String",
					"description": "单据创建人用户id，阿里工号",
					"required": true
				},
				{
					"name": "outer_travel_head_id",
					"type": "String",
					"description": "外部差旅申请单id，用作数据同步主键",
					"required": true
				},
				{
					"name": "peer_staff_list",
					"type": "PeerStaff[]",
					"description": "同行人列表",
					"subParams": [
						{
							"name": "user_id",
							"type": "String",
							"description": "同行人用户id，阿里工号",
							"required": true
						},
						{
							"name": "user_name",
							"type": "String",
							"description": "同行人姓名",
							"required": true
						}
					]
				},
				{
					"name": "status",
					"type": "String",
					"description": "单据审批状态， APPROVING：审批中,     APPROVED：已审批,     REJECTED：已拒绝",
					"required": true
				},
				{
					"name": "submit_user_id",
					"type": "String",
					"description": "单据实际申请人用户id，阿里工号",
					"required": true
				},
				{
					"name": "travel_purpose",
					"type": "String",
					"description": "出差原因说明",
					"required": true
				},
				{
					"name": "travel_type",
					"type": "String",
					"description": "差旅类型，   ORDINARY：出差（内部项目\u0026会议等），      RECRUITING：出差（外部合作\u0026交流等），      CONFERENCE：外部招待，     MARKETING：公司大型活动，     PUBLIC_RELATIONSHIP：入职\u0026候选人面试，   Training_Lecture：其它",
					"required": true
				},
				{
					"name": "line_list",
					"type": "TravelLineDto[]",
					"description": "差旅行程列表",
					"required": true,
					"subParams": [
						{
							"name": "begin_date",
							"type": "Date",
							"description": "开始时间",
							"required": true
						},
						{
							"name": "end_date",
							"type": "Date",
							"description": "结束时间",
							"required": true
						},
						{
							"name": "from_city",
							"type": "String",
							"description": "出发城市",
							"required": true
						},
						{
							"name": "itinerary_type",
							"type": "String",
							"description": "行程类型，ONE_WAY：单程，ROUND_TRIP：往返，MULTI_WAY：多程",
							"required": true
						},
						{
							"name": "to_city",
							"type": "String",
							"description": "目的地城市",
							"required": true
						},
						{
							"name": "transport_type",
							"type": "String",
							"description": "交通类型，PLANE：飞机，TRAIN：火车，AUTOMOTIVE：汽车，STEAMSHIP：轮船，OTHER：其他",
							"required": true
						},
						{
							"name": "travel_purpose",
							"type": "String",
							"description": "行程说明"
						}
					]
				},
				{
					"name": "submit_date",
					"type": "Date",
					"description": "申请单提交时间",
					"required": true
				}
			]
		}
	],
	"responseParams": [
		{
			"name": "travel_id",
			"type": "Number",
			"description": "差旅申请单在欢行内部产生的差旅单ID"
		}
	]
}