{
	"name": "alibaba.idle.agreement.pay",
	"apiChineseName": "闲鱼平台商户代扣",
	"description": "闲鱼平台代扣能力：用户和闲鱼签约代扣协议 服务商通过直付通产品挂载成为闲鱼二级商户 来完成用户和服务商的结算",
	"requestParams": [
		{
			"name": "agreement_pay_param",
			"type": "AgreementPayParam",
			"description": "协议代扣参数",
			"subParams": [
				{
					"name": "biz_order_id",
					"type": "String",
					"description": "商户订单号（唯一建）",
					"required": true
				},
				{
					"name": "pay_biz_code",
					"type": "String",
					"description": "R1:闲鱼回收代扣（V1版本）",
					"required": true
				},
				{
					"name": "plan_id",
					"type": "String",
					"description": "代扣计划",
					"required": true
				}
			]
		}
	],
	"responseParams": [
		{
			"name": "result",
			"type": "Result",
			"description": "接口返回model",
			"subParams": [
				{
					"name": "success",
					"type": "Boolean",
					"description": "是否成功",
					"required": true
				},
				{
					"name": "err_code",
					"type": "String",
					"description": "错误code",
					"required": true
				},
				{
					"name": "module",
					"type": "Serializable",
					"description": "服务出参",
					"required": true,
					"subParams": [
						{
							"name": "biz_order_id",
							"type": "String",
							"description": "商户订单号（唯一建）",
							"required": true
						},
						{
							"name": "alipay_trade_no",
							"type": "String",
							"description": "支付宝流水号",
							"required": true
						},
						{
							"name": "pay_time",
							"type": "String",
							"description": "支付时间",
							"required": true
						},
						{
							"name": "total_amount",
							"type": "Number",
							"description": "申请扣款金额（单位分）",
							"required": true
						},
						{
							"name": "receipt_amount",
							"type": "Number",
							"description": "实际支付金额（单位分）",
							"required": true
						},
						{
							"name": "pay_status",
							"type": "String",
							"description": "INIT:初始状态;PARTIAL_SUCCESSED:支付部分成功;SUCCESSED:支付成功;FAILED:支付失败",
							"required": true
						},
						{
							"name": "plan_id",
							"type": "String",
							"description": "代扣计划",
							"required": true
						}
					]
				},
				{
					"name": "err_msg",
					"type": "String",
					"description": "错误信息",
					"required": true
				}
			]
		}
	]
}