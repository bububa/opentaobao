{
	"name": "taobao.idle.recycle.refund.detail",
	"apiChineseName": "闲鱼回收退款详情V2",
	"description": "回收订单退款详情，主要包括退款状态，超时时间，和同意退款的卖家退货地址信息",
	"requestParams": [
		{
			"name": "biz_order_id",
			"type": "Number",
			"description": "订单号",
			"required": true
		}
	],
	"responseParams": [
		{
			"name": "result",
			"type": "IdleTopResult",
			"description": "退款详情，说明文档：https://www.yuque.com/tushuguanyuan/fsgl7y/xn2lu8#ed2c2d6a",
			"subParams": [
				{
					"name": "err_code",
					"type": "String",
					"description": "错误码",
					"required": true
				},
				{
					"name": "success",
					"type": "Boolean",
					"description": "成功",
					"required": true
				},
				{
					"name": "module",
					"type": "RecycleRefundDetailDto",
					"description": "退款详情",
					"required": true,
					"subParams": [
						{
							"name": "op_timeout",
							"type": "String",
							"description": "毫秒，操作超时时间，截止时间",
							"required": true
						},
						{
							"name": "biz_order_id",
							"type": "String",
							"description": "订单号",
							"required": true
						},
						{
							"name": "refund_status",
							"type": "String",
							"description": "状态(1, \"买家已经申请退款，等待卖家同意\"),  (2, \"卖家已经同意退货退款，等待买家退货\"), --该状态下卖家已经提供退货地址，同意退货退款和发送地址是同时的  (3, \"买家已经退货，等待卖家确认收货\"),  (4, \"退款关闭\"),  (5, \"退款成功\"), --仅退款情况下是从状态1变更到5，同意退款和退款是同时的  (6, \"卖家拒绝退款\"),  (7, \"等待买家确认重新邮寄的货物\"),--如补寄等情形，一般不会出现  (8, \"等待卖家确认退货地址\")",
							"required": true
						},
						{
							"name": "refund_status_desc",
							"type": "String",
							"description": "退款状态描述",
							"required": true
						},
						{
							"name": "refund_fee",
							"type": "String",
							"description": "退款金额",
							"required": true
						},
						{
							"name": "refund_desc",
							"type": "String",
							"description": "退款描述",
							"required": true
						},
						{
							"name": "apply_reason",
							"type": "String",
							"description": "申请原因",
							"required": true
						},
						{
							"name": "refuse_reason",
							"type": "String",
							"description": "拒绝原因",
							"required": true
						},
						{
							"name": "seller_address",
							"type": "ShippingAddressInfo",
							"description": "地址信息，交易状态为2时返回此字段",
							"required": true,
							"subParams": [
								{
									"name": "consignee_name",
									"type": "String",
									"description": "收件人姓名",
									"required": true
								},
								{
									"name": "address_detail",
									"type": "String",
									"description": "详细地址",
									"required": true
								},
								{
									"name": "mobile_phone",
									"type": "String",
									"description": "手机号",
									"required": true
								},
								{
									"name": "tel_no",
									"type": "String",
									"description": "电话",
									"required": true
								},
								{
									"name": "province",
									"type": "String",
									"description": "省",
									"required": true
								},
								{
									"name": "city",
									"type": "String",
									"description": "市",
									"required": true
								},
								{
									"name": "area",
									"type": "String",
									"description": "区",
									"required": true
								},
								{
									"name": "town",
									"type": "String",
									"description": "镇",
									"required": true
								},
								{
									"name": "post_code",
									"type": "String",
									"description": "邮编",
									"required": true
								}
							]
						},
						{
							"name": "actual_refund_fee",
							"type": "Number",
							"description": "实际退款金额，不包括追缴单",
							"required": true
						},
						{
							"name": "refund_end_time",
							"type": "String",
							"description": "退款完结时间",
							"required": true
						},
						{
							"name": "refund_start_time",
							"type": "String",
							"description": "退款开始时间",
							"required": true
						},
						{
							"name": "seller_agree_msg",
							"type": "String",
							"description": "卖家同意退货说明",
							"required": true
						},
						{
							"name": "recover_order_info",
							"type": "AlipayOrderDto",
							"description": "追缴单",
							"required": true,
							"subParams": [
								{
									"name": "amount",
									"type": "Number",
									"description": "金额",
									"required": true
								},
								{
									"name": "pay_status",
									"type": "Number",
									"description": "支付状态，1未支付，6已支付",
									"required": true
								},
								{
									"name": "alipay_trade_no",
									"type": "String",
									"description": "支付宝交易号",
									"required": true
								},
								{
									"name": "pay_order_id",
									"type": "String",
									"description": "支付订单",
									"required": true
								},
								{
									"name": "create_time",
									"type": "String",
									"description": "创建时间",
									"required": true
								},
								{
									"name": "pay_time",
									"type": "String",
									"description": "支付时间",
									"required": true
								}
							]
						}
					]
				},
				{
					"name": "err_msg",
					"type": "String",
					"description": "错误信息",
					"required": true
				}
			]
		}
	]
}