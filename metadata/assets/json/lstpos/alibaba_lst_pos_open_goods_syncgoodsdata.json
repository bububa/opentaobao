{
	"name": "alibaba.lst.pos.open.goods.syncgoodsdata",
	"apiChineseName": "门店商品批量同步接口(最多10条商品信息)",
	"description": "门店商品批量同步接口(最多10条商品信息)",
	"requestParams": [
		{
			"name": "goods_d_t_o_list",
			"type": "GoodsDto[]",
			"description": "商品对象列表",
			"required": true,
			"maxListSize": 10,
			"subParams": [
				{
					"name": "gmt_modified",
					"type": "Number",
					"description": "修改时间"
				},
				{
					"name": "gmt_create",
					"type": "Number",
					"description": "创建时间",
					"required": true
				},
				{
					"name": "bar_code",
					"type": "String",
					"description": "商品条码,唯一"
				},
				{
					"name": "title",
					"type": "String",
					"description": "商品标题",
					"required": true
				},
				{
					"name": "original_price",
					"type": "Number",
					"description": "进货价 ，单位为分",
					"required": true
				},
				{
					"name": "reserve_price",
					"type": "Number",
					"description": "零售价，单位为分",
					"required": true
				},
				{
					"name": "shortcut_code",
					"type": "String",
					"description": "快捷码(非必须)"
				},
				{
					"name": "shot_title",
					"type": "String",
					"description": "简称"
				},
				{
					"name": "spec",
					"type": "String",
					"description": "规格"
				},
				{
					"name": "unit",
					"type": "String",
					"description": "单位"
				},
				{
					"name": "status",
					"type": "String",
					"description": "商品状态 售卖中:sale 不可售卖：not sell"
				},
				{
					"name": "pricing_mode",
					"type": "String",
					"description": "计价方式  称重商品: weigh 非称重: non weigh"
				},
				{
					"name": "isv_category_id",
					"type": "String",
					"description": "自营叶子类目自定义编号(ISV子定义类目Id)"
				},
				{
					"name": "category_id",
					"type": "String",
					"description": "类目Id  1,洗涤用品; 2,家庭日用品; 3,小食品; 4,粮油调味; 5,酒水饮料; 6,速食; 7,生鲜; 8,服务性商品; 9,增值服务; 10,非商品收入;98,无码; 99,其他"
				},
				{
					"name": "isv_goods_id",
					"type": "String",
					"description": "ISV商品Id(保存商品入参用到)",
					"required": true
				}
			]
		},
		{
			"name": "user_id",
			"type": "Number",
			"description": "用户主账号Id",
			"required": true
		}
	],
	"responseParams": [
		{
			"name": "result",
			"type": "ResultDto",
			"description": "结果对象",
			"subParams": [
				{
					"name": "error_message",
					"type": "String",
					"description": "错误信息描述",
					"required": true
				},
				{
					"name": "modules",
					"type": "ErrorResult[]",
					"description": "接口具体返回的业务数据对象",
					"required": true,
					"subParams": [
						{
							"name": "key",
							"type": "String",
							"description": "主键或唯一标识",
							"required": true
						},
						{
							"name": "error_code",
							"type": "String",
							"description": "错误码：0成功1失败",
							"required": true
						},
						{
							"name": "error_message",
							"type": "String",
							"description": "错误信息",
							"required": true
						},
						{
							"name": "success",
							"type": "Boolean",
							"description": "单个商品处理结果 true：成功，false：失败",
							"required": true
						},
						{
							"name": "data",
							"type": "Json",
							"description": "单个业务处理数据",
							"required": true
						}
					]
				},
				{
					"name": "error_code",
					"type": "String",
					"description": "业务错误码",
					"required": true
				},
				{
					"name": "success",
					"type": "Boolean",
					"description": "接口调用是否成功 true:调用成功 false:调用失败",
					"required": true
				}
			]
		}
	]
}