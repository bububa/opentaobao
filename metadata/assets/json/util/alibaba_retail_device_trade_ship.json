{
	"name": "alibaba.retail.device.trade.ship",
	"apiChineseName": "贩卖机掉货成功通知",
	"description": "贩卖机发货",
	"requestParams": [
		{
			"name": "device_type",
			"type": "String",
			"description": "设备类型",
			"required": true
		},
		{
			"name": "device_id",
			"type": "String",
			"description": "设备ID",
			"required": true
		},
		{
			"name": "trade_no",
			"type": "String",
			"description": "内部订单编号",
			"required": true
		},
		{
			"name": "ship_detail_list",
			"type": "ShipDetailDTO[]",
			"description": "详情",
			"required": true,
			"maxListSize": 20,
			"subParams": [
				{
					"name": "item_id",
					"type": "Number",
					"description": "商品",
					"required": true
				},
				{
					"name": "ship_status",
