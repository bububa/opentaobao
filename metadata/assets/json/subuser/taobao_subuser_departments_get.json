{
	"id": 21689,
	"name": "taobao.subuser.departments.get",
	"apiChineseName": "获取指定账户的所有部门列表",
	"description": "获取指定账户的所有部门列表，其实包括有每个部门的ID、父部门ID、部门名称（通过主账号登陆只能查询属于该主账号下的所有部门信息）。",
	"requestParams": [
		{
			"name": "user_nick",
			"type": "String",
			"description": "主账号用户名",
			"required": true
		}
	],
	"responseParams": [
		{
			"name": "departments",
			"type": "Department []",
			"description": "部门信息",
			"subParams": [
				{
					"name": "department_id",
					"type": "Number",
					"description": "部门ID"
				},
				{
					"name": "parent_id",
					"type": "Number",
					"description": "当前部门的父部门ID"
				},
				{
					"name": "department_name",
					"type": "String",
					"description": "部门名称"
				},
				{
					"name": "sub_user_ids",
					"type": "Number[]",
					"description": "部门下关联的子账号id列表"
				}
			]
		}
	]
}