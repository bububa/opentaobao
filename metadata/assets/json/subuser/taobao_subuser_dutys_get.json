{
	"id": 21696,
	"name": "taobao.subuser.dutys.get",
	"apiChineseName": "获取指定账户的所有职务信息列表",
	"description": "通过主账号Nick获取该账户下的所有职务信息，职务信息中包括职务ID、职务名称以及职务等级（通过主账号登陆只能获取属于该主账号下的职务信息）",
	"requestParams": [
		{
			"name": "user_nick",
			"type": "String",
			"description": "主账号用户名",
			"required": true
		}
	],
	"responseParams": [
		{
			"name": "dutys",
			"type": "Duty []",
			"description": "职务信息",
			"subParams": [
				{
					"name": "duty_id",
					"type": "Number",
					"description": "职务ID"
				},
				{
					"name": "duty_name",
					"type": "String",
					"description": "职务名称"
				},
				{
					"name": "duty_level",
