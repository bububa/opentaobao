{
	"name": "alibaba.wdk.marketing.coupon.additem",
	"apiChineseName": "在优惠券活动下添加商品。【注意，此接口暂不支持并发！】",
	"description": "在优惠券活动下添加商品。【注意，此接口暂不支持并发！】\n如果是商品券，则添加的商品为券适用的商品；\n如果是品类券，则添加的商品为券排除的商品；",
	"requestParams": [
		{
			"name": "param0",
			"type": "ItemCouponSku",
			"description": "商品对象",
			"required": true,
			"subParams": [
				{
					"name": "sku_code",
					"type": "String",
					"description": "商品的skuCode",
					"required": true
				}
			]
		},
		{
			"name": "param1",
			"type": "CommonActivityParam",
			"description": "活动基本信息",
			"required": true,
			"subParams": [
				{
					"name": "activity_id",
					"type": "Number",
					"description": "五道口活动id",
					"required": true
				},
				{
					"name": "out_act_id",
					"type": "String",
					"description": "商家活动id"
				}
			]
		}
	],
	"responseParams": [
		{
			"name": "result",
			"type": "MarketResult",
			"description": "商品报名活动的返回结果",
			"subParams": [
				{
					"name": "message",
					"type": "String",
					"description": "错误信息",
					"required": true
				},
				{
					"name": "error_code",
					"type": "String",
					"description": "错误码",
					"required": true
				},
				{
					"name": "data",
					"type": "ItemCouponSku",
					"description": "报名活动成功的商品详情",
					"required": true,
					"subParams": [
						{
							"name": "sku_name",
							"type": "String",
							"description": "商品名称",
							"required": true
						},
						{
							"name": "sku_code",
							"type": "String",
							"description": "商品的skuCode",
							"required": true
						},
						{
							"name": "item_shop_relation",
							"type": "Json",
							"description": "淘宝item和shop的对应关系， k-itemId, v-shopId",
							"required": true
						}
					]
				},
				{
					"name": "success",
					"type": "Boolean",
					"description": "是否报名成功",
					"required": true
				}
			]
		}
	]
}