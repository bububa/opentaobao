						{
							"name": "barcodes",
							"type": "String[]",
							"description": "条码",
							"required": true
						},
						{
							"name": "life_status",
							"type": "String",
							"description": "商品生命周期状态(A-正常、T-暂时停购、C-淘汰出清、R-清退、D-删除封挡)",
							"required": true
						},
						{
							"name": "back_cat_code",
							"type": "String",
							"description": "平台类目编码",
							"required": true
						},
						{
							"name": "retailer_cat_code",
