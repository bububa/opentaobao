						{
							"name": "product_code",
							"type": "String",
							"description": "产品标准号",
							"required": true
						},
						{
							"name": "factory_no",
							"type": "String",
							"description": "厂方货号",
							"required": true
						},
						{
							"name": "component",
							"type": "String",
