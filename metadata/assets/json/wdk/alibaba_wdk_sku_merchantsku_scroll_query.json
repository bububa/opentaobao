{
	"name": "alibaba.wdk.sku.merchantsku.scroll.query",
	"apiChineseName": "商家商品批量查询接口",
	"description": "提供主档商品数据接口查询",
	"requestParams": [
		{
			"name": "org_no",
			"type": "String",
			"description": "HM"
		},
		{
			"name": "scroll_id",
			"type": "String",
			"description": "第一次为null，后面从结果中获取"
		}
	],
	"responseParams": [
		{
			"name": "result",
			"type": "ApiScrollPageResult",
			"description": "请求结果对象",
			"subParams": [
				{
					"name": "total",
					"type": "Number",
					"description": "条目总数",
					"required": true
				},
				{
					"name": "scroll_id",
					"type": "String",
					"description": "第一次调用会返回scroll_id 但并不包含数据，后续每次都必须指定上一次返回scroll_id，并且后续搜索结果中都会返回scroll_id及对应匹配的数据，后续查询该参数必填",
					"required": true
				},
				{
					"name": "page_count",
					"type": "Number",
					"description": "结果页数",
					"required": true
				},
				{
					"name": "model_list",
					"type": "WdkOpenMerchantSkuDo[]",
