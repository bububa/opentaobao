							"description": "商家类目编码",
							"required": true
						},
						{
							"name": "item_type",
							"type": "Number",
							"description": "商品经营方式(1001-普通商品， 2001-加工成品，2002-加工半成品，3001-原材料，4001-耗材，6001-组合商品)",
							"required": true
						},
						{
							"name": "invoice_content",
							"type": "String",
							"description": "商品销项税率",
							"required": true
						},
						{
							"name": "sale_flag",
