{
	"name": "alibaba.ascp.uop.supplier.consignorder.notify.tms.change",
	"apiChineseName": "商家修改运单号",
	"description": "供应商可以通过此接口，对出库回告上报的运单号进行修改，目前一次调用只能支持一个运单号的修改",
	"requestParams": [
		{
			"name": "modify_mail_no_request",
			"type": "Modifymailnorequest",
			"description": "修改运单号请求模型",
			"required": true,
			"subParams": [
				{
					"name": "supplier_id",
					"type": "String",
					"description": "供应商id",
					"required": true
				},
				{
					"name": "old_tms_order_code",
					"type": "String",
					"description": "原运单号",
					"required": true
				},
				{
					"name": "new_tms_order_code",
					"type": "String",
					"description": "新运单号",
					"required": true
				},
				{
					"name": "old_tms_service_code",
					"type": "String",
					"description": "原配送公司编码",
					"required": true
				},
				{
					"name": "biz_order_code",
					"type": "String",
					"description": "发货LP",
					"required": true
				}
			]
		}
	],
	"responseParams": [
		{
			"name": "modify_mail_no_response",
			"type": "ResultWrapper",
			"description": "返回值包装,result为返回具体消息内容",
			"subParams": [
				{
					"name": "success",
					"type": "Boolean",
					"description": "返回是否成功",
					"required": true
				},
				{
					"name": "error_message",
					"type": "String",
					"description": "错误描述",
					"required": true
				},
				{
					"name": "error_code",
					"type": "String",
					"description": "错误编码",
					"required": true
				},
				{
					"name": "retry",
					"type": "Boolean",
					"description": "是否retry",
					"required": true
				}
			]
		}
	]
}