			"name": "consignorder_ship_request",
			"type": "Consignordershiprequest",
			"description": "发货回传请求模型",
			"subParams": [
				{
					"name": "supplier_id",
					"type": "String",
					"description": "供应商id",
					"required": true
				},
				{
					"name": "out_biz_id",
					"type": "String",
					"description": "外部业务号，幂等控制使用",
					"required": true
				},
				{
					"name": "biz_order_code",
					"type": "String",
					"description": "履约单号",
					"required": true
				},
				{
					"name": "order_items",
					"type": "Orderitems[]",
					"description": "履约子单明细",
					"required": true,
					"subParams": [
						{
							"name": "sub_order_code",
							"type": "String",
							"description": "履约子单号",
							"required": true
						},
						{
							"name": "sc_item_id",
							"type": "String",
							"description": "货品ID"
						},
						{
							"name": "item_quantity",
							"type": "Number",
							"description": "货品实发数量",
							"required": true
						},
						{
							"name": "lack_quantity",
							"type": "Number",
							"description": "货品缺发数量"
						}
					]
				},
				{
					"name": "store_code",
					"type": "String",
					"description": "发货仓编码"
				},
				{
					"name": "store_name",
					"type": "String",
					"description": "发货仓名称"
