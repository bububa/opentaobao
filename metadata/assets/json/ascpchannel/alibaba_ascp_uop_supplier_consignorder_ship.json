							"required": true
						},
						{
							"name": "tms_service_name",
							"type": "String",
							"description": "快递公司名称"
						},
						{
							"name": "tms_items",
							"type": "Tmsitems[]",
							"description": "包裹明细列表",
							"required": true,
							"subParams": [
								{
									"name": "sub_order_code",
									"type": "String",
									"description": "履约子单号",
									"required": true
								},
								{
									"name": "sc_item_id",
									"type": "String",
									"description": "货品ID"
								},
								{
									"name": "item_quantity",
									"type": "Number",
									"description": "货品实发数量",
									"required": true
								},
								{
									"name": "lack_quantity",
									"type": "Number",
									"description": "货品缺发数量"
								}
							]
						}
					]
				},
