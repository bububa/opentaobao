						{
							"name": "sender_country",
							"type": "String",
							"description": "发件方国家"
						},
						{
							"name": "sender_province",
							"type": "String",
							"description": "发件方省份"
						},
						{
							"name": "sender_city",
							"type": "String",
							"description": "发件方城市"
						},
						{
							"name": "sender_area",
							"type": "String",
							"description": "发件方区县"
						},
						{
							"name": "sender_town",
							"type": "String",
							"description": "发件方镇村"
						},
						{
							"name": "sender_address",
							"type": "String",
							"description": "发件方地址"
						},
						{
							"name": "sender_name",
							"type": "String",
							"description": "发件方名称"
						},
						{
							"name": "sender_mobile",
							"type": "String",
							"description": "发件方手机"
						},
						{
							"name": "sender_phone",
							"type": "String",
							"description": "发件方电话"
						}
					]
				},
				{
					"name": "whole_sheet_consigned",
					"type": "Boolean",
					"description": "是否整单发货,目前只支持履约单整单发货回传"
				}
			]
		}
	],
	"responseParams": [
		{
			"name": "consignorder_ship_response",
			"type": "ResultWrapper",
			"description": "返回值包装,result为返回具体消息内容",
			"subParams": [
				{
					"name": "success",
					"type": "Boolean",
					"description": "返回是否成功",
					"required": true
				},
				{
					"name": "error_code",
					"type": "String",
					"description": "系统自动生成",
					"required": true
				},
				{
					"name": "error_message",
					"type": "String",
					"description": "系统自动生成",
					"required": true
				}
			]
		}
	]
}