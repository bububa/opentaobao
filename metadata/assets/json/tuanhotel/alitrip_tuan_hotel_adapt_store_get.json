					"description": "核销账户",
					"required": true
				},
				{
					"name": "shid",
					"type": "Number",
					"description": "标准酒店ID",
					"required": true
				},
				{
					"name": "name",
					"type": "String",
					"description": "名字",
					"required": true
				},
				{
					"name": "appoint_phone_list",
					"type": "AppointPhoneVoList[]",
					"description": "联系电话列表",
