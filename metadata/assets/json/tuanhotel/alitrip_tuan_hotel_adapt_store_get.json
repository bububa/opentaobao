				},
				{
					"name": "bill_infos",
					"type": "String",
					"description": "分账信息",
					"required": true
				},
				{
					"name": "store_id",
					"type": "Number",
					"description": "门店id",
					"required": true
				},
				{
					"name": "room_type_list",
					"type": "RoomTypeVoList[]",
					"description": "房型列表",
					"required": true,
					"subParams": [
						{
							"name": "network_service",
