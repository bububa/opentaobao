		{
			"name": "shid_list",
			"type": "Number[]",
			"description": "标准酒店ID列表,逗号分割。与hid_list二者只能选一",
			"maxListSize": 20
		},
		{
			"name": "hid_list",
			"type": "Number[]",
			"description": "物理酒店ID列表，逗号分割。与shid_list二者只能选一",
			"maxListSize": 20
		}
	],
	"responseParams": [
		{
			"name": "store_detail_list",
			"type": "StoreDetailVoList []",
			"description": "关联门店列表",
			"subParams": [
				{
					"name": "hid",
					"type": "String",
					"description": "物理酒店ID",
					"required": true
				},
				{
					"name": "bill_descs",
					"type": "String",
					"description": "分账描述",
					"required": true
				},
