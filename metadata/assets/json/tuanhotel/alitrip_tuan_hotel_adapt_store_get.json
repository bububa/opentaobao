						},
						{
							"name": "bed",
							"type": "String",
							"description": "床型",
							"required": true
						},
						{
							"name": "name",
							"type": "String",
							"description": "名称",
							"required": true
						},
						{
							"name": "max_occupancy",
							"type": "String",
