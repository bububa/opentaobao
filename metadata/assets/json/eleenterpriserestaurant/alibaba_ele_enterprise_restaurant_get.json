{
	"name": "alibaba.ele.enterprise.restaurant.get",
	"apiChineseName": "查询餐厅信息",
	"description": "查询餐厅信息",
	"requestParams": [
		{
			"name": "geo",
			"type": "String",
			"description": "longitude和latitude用英文逗号分隔",
			"required": true
		},
		{
			"name": "erestaurant_id",
			"type": "String",
			"description": "餐厅ID",
			"required": true
		}
	],
	"responseParams": [
		{
			"name": "enterprise_code",
			"type": "String",
			"description": "响应code"
		},
		{
			"name": "enterprise_msg",
			"type": "String",
			"description": "响应信息"
		},
		{
			"name": "enterprise_data",
			"type": "EnterpriseData",
			"description": "返回餐厅信息",
			"subParams": [
				{
					"name": "distance",
					"type": "String",
					"description": "距离（米）",
					"required": true
				},
				{
					"name": "rating",
					"type": "String",
					"description": "店铺综合评分",
					"required": true
				},
				{
					"name": "only_restaurant_code",
					"type": "String",
					"description": "餐厅唯一码",
					"required": true
				},
				{
					"name": "promotion_info",
					"type": "String",
					"description": "促销信息或者商家公告",
					"required": true
				},
				{
					"name": "recent_order_num",
					"type": "Number",
					"description": "最近一个月订单量",
					"required": true
				},
				{
					"name": "agent_fee",
					"type": "String",
					"description": "配送费",
					"required": true
				},
				{
					"name": "deliver_spent",
					"type": "Number",
					"description": "2周内的平均送餐时间",
					"required": true
				},
				{
					"name": "is_open",
					"type": "Number",
					"description": "是否正在营业, 在营业的前提下，再通过total_status字段判断商家的具体营业状态 餐厅整体营业状态",
					"required": true
				},
				{
					"name": "is_dist_rst",
					"type": "Number",
					"description": "是否蜂鸟专送餐厅",
					"required": true
				},
				{
					"name": "restaurant_name",
					"type": "String",
					"description": "餐厅名称",
					"required": true
				},
				{
					"name": "activities",
					"type": "Activitie[]",
					"description": "活动, 参考餐厅活动",
					"required": true,
					"subParams": [
						{
							"name": "detail_type",
							"type": "Number",
							"description": "见附录【活动信息detail_type值】",
							"required": true
						},
						{
							"name": "name",
							"type": "String",
							"description": "活动名称",
							"required": true
						},
						{
							"name": "description",
							"type": "String",
							"description": "餐厅描述",
							"required": true
						},
						{
							"name": "id",
							"type": "Number",
							"description": "活动ID",
							"required": true
						}
					]
				},
				{
					"name": "image_url",
					"type": "String",
					"description": "餐厅Logo地址",
					"required": true
				},
				{
					"name": "deliver_amount",
					"type": "String",
					"description": "起送价",
					"required": true
				},
				{
					"name": "erestaurant_id",
					"type": "String",
					"description": "餐厅ID",
					"required": true
				},
				{
					"name": "invoice",
					"type": "Number",
					"description": "是否支持开发票: 1, 是; 2, 否.",
					"required": true
				},
				{
					"name": "serial_number",
					"type": "String",
					"description": "序列号（无业务含义）",
					"required": true
				},
				{
					"name": "is_bookable",
					"type": "Number",
					"description": "是否支持预定. 0 不支持预定, 1 支持预定",
					"required": true
				},
				{
					"name": "is_new",
					"type": "Number",
					"description": "是否新店：0,否；1,是",
					"required": true
				},
				{
					"name": "is_premium",
					"type": "Number",
					"description": "是否品牌店：0，否；1，是",
					"required": true
				},
				{
					"name": "is_insurance",
					"type": "String",
					"description": "是否支持食安保：0，否；1，是",
					"required": true
				},
				{
					"name": "address_text",
					"type": "String",
					"description": "餐厅地址",
					"required": true
				},
				{
					"name": "serving_times",
					"type": "String[]",
					"description": "餐厅营业时间",
					"required": true
				},
				{
					"name": "phone_list",
					"type": "String[]",
					"description": "餐厅电话",
					"required": true
				},
				{
					"name": "average_cost",
					"type": "String",
					"description": "人均消费金额（元）",
					"required": true
				},
				{
					"name": "total_status",
					"type": "Number",
					"description": "餐厅整体营业状态：1 餐厅营业中，2餐厅关闭，3 餐厅网路不稳定，4 餐厅休息中，5 直接说预定，6 只接受电话预定，7 餐厅休假中",
					"required": true
				}
			]
		},
		{
			"name": "enterprise_requestid",
			"type": "String",
			"description": "请求id"
		}
	]
}