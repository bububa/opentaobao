{
	"name": "alibaba.westcrm.activity.list.get",
	"apiChineseName": "获取活动列表接口",
	"description": "获取活动列表提供给友盟\u0026互动屏",
	"requestParams": [
		{
			"name": "status",
			"type": "Number",
			"description": "活动状态"
		},
		{
			"name": "campus_id",
			"type": "Number",
			"description": "园区id"
		},
		{
			"name": "sord",
			"type": "String",
			"description": "排序方向"
		},
		{
			"name": "page",
			"type": "Number",
			"description": "页,默认第一页"
		},
		{
			"name": "sidx",
			"type": "String",
			"description": "排序字段"
		},
		{
			"name": "offset",
			"type": "Number",
			"description": "分页偏移量 eq . limit offset ,rows"
		},
		{
			"name": "rows",
			"type": "Number",
			"description": "页大小,默认每页查询10条数据"
		}
	],
	"responseParams": [
		{
			"name": "result",
			"type": "DataResult",
			"description": "返回对象封装",
			"subParams": [
				{
					"name": "message",
					"type": "String",
					"description": "结果信息",
					"required": true
				},
				{
					"name": "data",
					"type": "ThirdpartyPageData",
					"description": "返回数据",
					"required": true,
					"subParams": [
						{
							"name": "current",
							"type": "Number",
							"description": "当前页",
							"required": true
						},
						{
							"name": "list",
							"type": "ActivitiesListVo[]",
							"description": "活动列表",
							"required": true,
							"subParams": [
								{
									"name": "time_end",
									"type": "Date",
									"description": "活动有效期-结束时间",
									"required": true
								},
								{
									"name": "pic_url",
									"type": "String",
									"description": "头图地址",
									"required": true
								},
								{
									"name": "status",
									"type": "Number",
									"description": "活动状态：1-未开始；2-进行中；3-已结束",
									"required": true
								},
								{
									"name": "tags",
									"type": "ThirdTagsVo",
									"description": "目标客群结构体",
									"required": true,
									"subParams": [
										{
											"name": "tags_age_list",
											"type": "Json[]",
											"description": "目标客群年龄段",
											"required": true
										},
										{
											"name": "tags_money_list",
											"type": "Json[]",
											"description": "目前客群收入",
											"required": true
										},
										{
											"name": "tags_grader_list",
											"type": "Json[]",
											"description": "目标客群等级",
											"required": true
										}
									]
								},
								{
									"name": "is_display",
									"type": "Number",
									"description": "活动是否在活动列表中展示",
									"required": true
								},
								{
									"name": "title",
									"type": "String",
									"description": "活动名称",
									"required": true
								},
								{
									"name": "activity_type",
									"type": "Number",
									"description": "活动类型：0商场活动，1互动活动",
									"required": true
								},
								{
									"name": "sort",
									"type": "Number",
									"description": "排序权重",
									"required": true
								},
								{
									"name": "campus_id",
									"type": "Number",
									"description": "园区id",
									"required": true
								},
								{
									"name": "time_begin",
									"type": "Date",
									"description": "活动有效期-开始时间",
									"required": true
								},
								{
									"name": "activity_id",
									"type": "Number",
									"description": "活动id",
									"required": true
								}
							]
						},
						{
							"name": "total",
							"type": "Number",
							"description": "总数",
							"required": true
						}
					]
				},
				{
					"name": "code",
					"type": "Number",
					"description": "参数code",
					"required": true
				}
			]
		}
	]
}