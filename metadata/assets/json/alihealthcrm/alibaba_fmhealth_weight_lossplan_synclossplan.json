	"description": "减重计划--三方同步用户初始化减重计划给我们",
	"requestParams": [
		{
			"name": "tp_user_id",
			"type": "Number",
			"description": "用户id",
			"required": true
		},
		{
			"name": "gender",
			"type": "Number",
			"description": "性别0女 1男",
			"required": true
		},
		{
			"name": "birthday",
			"type": "String",
			"description": "生日",
			"required": true
		},
		{
			"name": "height",
			"type": "Number",
			"description": "身高170 即一米七",
			"required": true
		},
		{
			"name": "weight",
			"type": "String",
			"description": "当前体重（今天的体重），单位kg",
			"required": true
		},
		{
			"name": "type",
