{
	"name": "alibaba.alihealth.examination.reserve.state.notify",
	"apiChineseName": "体检机构对接_体检状态主动通知",
	"description": "到了体检当天后，服务商主动通知体检预约状态",
	"requestParams": [
		{
			"name": "uniq_reserve_code",
			"type": "String",
			"description": "服务商预约凭证",
			"required": true
		},
		{
			"name": "reserve_number",
			"type": "String",
			"description": "健康预约凭证",
			"required": true
		},
		{
			"name": "report_status",
			"type": "String",
			"description": "体检状态：未到检(exam_not), 已到检(exam_done)； 上门服务中还需以下两种状态：预约确认中（reserve_confirming），预约拒绝（reserve_rejected）；",
			"required": true
		},
		{
			"name": "check_no",
			"type": "String",
			"description": "到检凭证，exam_done状态下，该字段必填"
		}
	],
	"responseParams": [
		{
			"name": "result",
			"type": "ServiceResult",
			"description": "alinkappserver系统返回的通用结果类",
			"subParams": [
				{
					"name": "eagle_eye_trace_id",
					"type": "String",
					"description": "鹰眼",
					"required": true
				},
				{
					"name": "data",
					"type": "ReserveStatusResultResponse",
					"description": "返回数据对象",
					"required": true,
					"subParams": [
						{
							"name": "response_code",
							"type": "String",
							"description": "业务响应code",
							"required": true
						},
						{
							"name": "msg",
							"type": "String",
							"description": "正文",
							"required": true
						}
					]
				},
				{
					"name": "success",
					"type": "Boolean",
					"description": "success",
					"required": true
				},
				{
					"name": "err_code",
					"type": "String",
					"description": "系统错误码",
					"required": true
				},
				{
					"name": "err_message",
					"type": "String",
					"description": "系统错误信息",
					"required": true
				}
			]
		}
	]
}