{
	"name": "alibaba.alihealth.examination.items.publish",
	"apiChineseName": "单项/加项包信息同步",
	"description": "体检机构对接_单项/加项包信息发布／更新",
	"requestParams": [
		{
			"name": "group_id",
			"type": "String",
			"description": "商品id，机构保证全局唯一"
		},
		{
			"name": "isv_packages",
			"type": "IsvPackage[]",
			"description": "套餐列表",
			"required": true,
			"maxListSize": 100,
			"subParams": [
				{
					"name": "package_code",
					"type": "String",
					"description": "套餐代码，机构保证全局唯一",
					"required": true
				},
				{
					"name": "add_isv_item_ids",
					"type": "String[]",
					"description": "套餐可用加项id列表"
				},
				{
					"name": "add_isv_pack_ids",
					"type": "String[]",
					"description": "套餐可用加项包id列表"
				},
				{
					"name": "basic_item_ids",
					"type": "String[]",
					"description": "套餐基础项id列表",
					"required": true
				}
			]
		},
		{
			"name": "isv_item_relation_d_t_o_s",
			"type": "IsvItemRelationDto[]",
			"description": "单项之间关系",
			"maxListSize": 1000,
			"subParams": [
				{
					"name": "isv_item_id",
					"type": "String",
					"description": "单项id"
				},
				{
					"name": "rel_isv_item_id",
					"type": "String",
					"description": "关联单项id"
				},
				{
					"name": "type",
					"type": "Number",
					"description": "关系：1、互斥 2、重复"
				}
			]
		},
		{
			"name": "hospital_codes",
			"type": "String[]",
			"description": "体检机构标识",
			"maxListSize": 500
		},
		{
			"name": "isv_item_pack_d_t_o_s",
			"type": "IsvItemPackDto[]",
			"description": "加项包列表",
			"maxListSize": 1000,
			"subParams": [
				{
					"name": "isv_pack_id",
					"type": "String",
					"description": "加项包id"
				},
				{
					"name": "pack_name",
					"type": "String",
					"description": "加项包名称"
				},
				{
					"name": "sold_price",
					"type": "String",
					"description": "售价，单位分"
				},
				{
					"name": "type",
					"type": "Number",
					"description": "类型：1、可选加项包 2、声明式加项包."
				},
				{
					"name": "isv_item_ids",
					"type": "String[]",
					"description": "加项包包含的单项id列表"
				},
				{
					"name": "version",
					"type": "Number",
					"description": "版本号，防止isv更改未同步给健康，提供给isv做校验的"
				},
				{
					"name": "settle_price",
					"type": "String",
					"description": "结算价，单位分"
				}
			]
		},
		{
			"name": "isv_item_d_t_o_s",
			"type": "IsvItemDto[]",
			"description": "单项信息列表",
			"required": true,
			"maxListSize": 2000,
			"subParams": [
				{
					"name": "isv_item_id",
					"type": "String",
					"description": "isv的单项id",
					"required": true
				},
				{
					"name": "detail",
					"type": "String",
					"description": "单项描述"
				},
				{
					"name": "sub_title",
					"type": "String",
					"description": "标题",
					"required": true
				},
				{
					"name": "version",
					"type": "Number",
					"description": "版本号，防止isv更改未同步给健康，提供给isv做校验的"
				},
				{
					"name": "sold_price",
					"type": "String",
					"description": "售价，单位分"
				}
			]
		},
		{
			"name": "isv_pack_relation_d_t_o_s",
			"type": "IsvPackRelationDto[]",
			"description": "加项包关系列表",
			"maxListSize": 2000,
			"subParams": [
				{
					"name": "isv_pack_id",
					"type": "String",
					"description": "加项包id"
				},
				{
					"name": "rel_isv_pack_id",
					"type": "String",
					"description": "关联加项包id"
				},
				{
					"name": "type",
					"type": "Number",
					"description": "关系：1、互斥."
				}
			]
		}
	],
	"responseParams": [
		{
			"name": "result",
			"type": "ServiceResult",
			"description": "返回结果",
			"subParams": [
				{
					"name": "err_message",
					"type": "String",
					"description": "详细错误信息",
					"required": true
				},
				{
					"name": "data",
					"type": "ItemsPublishResponse",
					"description": "返回结果说明信息",
					"required": true,
					"subParams": [
						{
							"name": "msg",
							"type": "String",
							"description": "结果说明",
							"required": true
						}
					]
				},
				{
					"name": "err_code",
					"type": "String",
					"description": "错误码",
					"required": true
				},
				{
					"name": "success",
					"type": "Boolean",
					"description": "执行成功标示",
					"required": true
				}
			]
		}
	]
}