			"required": true,
			"maxListSize": 20
		},
		{
			"name": "description",
			"type": "String",
			"description": "描述长度(9~200)",
			"required": true
		},
		{
			"name": "ext_json",
			"type": "String",
			"description": "schemadata, json字符串",
			"required": true
		},
		{
			"name": "icon",
			"type": "String",
			"description": "小程序icon",
			"required": true
		},
		{
			"name": "name",
			"type": "String",
			"description": "小程序名称",
			"required": true
		},
		{
			"name": "template_id",
			"type": "String",
			"description": "模板id",
			"required": true
		},
		{
			"name": "template_version",
			"type": "String",
			"description": "模板版本",
			"required": true
		}
