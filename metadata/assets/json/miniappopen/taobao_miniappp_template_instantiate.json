						{
							"name": "app_description",
							"type": "String",
							"description": "小程序描述",
							"required": true
						},
						{
							"name": "app_icon",
							"type": "String",
							"description": "icon",
							"required": true
						},
						{
							"name": "online_version",
							"type": "String",
							"description": "当前版本",
							"required": true
						},
						{
							"name": "online_code",
							"type": "String",
							"description": "线上码",
							"required": true
						}
					]
				},
				{
					"name": "success",
					"type": "Boolean",
					"description": "是否成功",
					"required": true
				}
			]
		}
	]
