				{
					"name": "err_code",
					"type": "String",
					"description": "错误码",
					"required": true
				},
				{
					"name": "err_message",
					"type": "String",
					"description": "错误信息",
					"required": true
				},
				{
					"name": "model",
					"type": "MiniAppEntityDto",
					"description": "model",
					"required": true,
