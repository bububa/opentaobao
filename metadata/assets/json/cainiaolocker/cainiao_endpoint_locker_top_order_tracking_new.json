			"required": true,
			"subParams": [
				{
					"name": "order_type",
					"type": "Number",
					"description": "订单类型(0-代收业务)",
					"required": true
				},
				{
					"name": "getter_phone",
					"type": "String",
					"description": "订单对应的取件人电话"
				},
				{
					"name": "post_phone",
					"type": "String",
					"description": "订单对应的投件人电话"
				},
				{
					"name": "extra",
