{
	"id": 37944,
	"name": "taobao.jst.astrolabe.storeinventory.query",
	"apiChineseName": "后端商品库存查询接口",
	"description": "查询门店或电商仓库存，该接口一次可以同时查询多个门店或电商仓的多个商品的多种类型的库存",
	"requestParams": [
		{
			"name": "stores",
			"type": "Store[]",
			"description": "门店",
			"maxListSize": 20,
			"subParams": [
				{
					"name": "store_inventories",
					"type": "StoreInventory[]",
					"description": "门店库存",
					"subParams": [
						{
							"name": "outer_id",
							"type": "String",
							"description": "ISV系统中商品编码"
						},
						{
							"name": "sc_item_id",
							"type": "Number",
							"description": "淘宝后端商品id",
							"required": true
						}
					]
				},
				{
					"name": "warehouse_id",
					"type": "String",
					"description": "门店ID(商户中心) 或 电商仓ID",
					"required": true
				},
				{
					"name": "warehouse_type",
					"type": "String",
					"description": "库存来源",
					"required": true
				}
			]
		}
	],
	"responseParams": [
		{
			"name": "flag",
			"type": "String",
			"description": "响应标示"
		},
		{
			"name": "result_code",
			"type": "String",
			"description": "响应编码"
		},
		{
			"name": "message",
			"type": "String",
			"description": "响应信息"
		},
		{
			"name": "stores",
			"type": "Store []",
			"description": "门店列表",
			"subParams": [
				{
					"name": "store_inventories",
					"type": "StoreInventory[]",
					"description": "门店库存信息",
					"required": true,
					"subParams": [
						{
							"name": "quantity_details",
							"type": "QuantityDetail[]",
							"description": "库存量详情",
							"required": true,
							"subParams": [
								{
									"name": "quantity",
									"type": "String",
									"description": "当前类型库存数量",
									"required": true
								},
								{
									"name": "inventory_type",
									"type": "String",
									"description": "库存类型",
									"required": true
								}
							]
						},
						{
							"name": "outer_id",
							"type": "String",
							"description": "ISV系统中商品编码",
							"required": true
						},
						{
							"name": "sc_item_id",
							"type": "String",
							"description": "淘宝后端商品id",
							"required": true
						}
					]
				},
				{
					"name": "warehouse_id",
					"type": "String",
					"description": "门店ID(商户中心) 或 电商仓ID",
					"required": true
				},
				{
					"name": "warehouse_type",
					"type": "String",
					"description": "库存来源",
					"required": true
				}
			]
		}
	]
}