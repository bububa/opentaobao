{
	"name": "taobao.omniitem.classify.operator",
	"apiChineseName": "添加/修改分类",
	"description": "添加/修改分类",
	"requestParams": [
		{
			"name": "category",
			"type": "OmniItemCategoryDetailDto",
			"description": "分类信息",
			"subParams": [
				{
					"name": "inner_name",
					"type": "String",
					"description": "内部",
					"required": true
				},
				{
					"name": "classify_id",
					"type": "Number",
					"description": "分类ID（添加时不太填，修改时要填）"
				},
				{
					"name": "display_name",
					"type": "String",
					"description": "展示名称",
					"required": true
				},
				{
					"name": "show_offline",
					"type": "Boolean",
					"description": "是否在线上显示",
					"required": true
				},
				{
					"name": "show_online",
					"type": "Boolean",
					"description": "是否在线下显示",
					"required": true
				}
			]
		},
		{
			"name": "add_item_ids",
			"type": "Number[]",
			"description": "需要添加的关联关系的商品",
			"maxListSize": 20
		},
		{
			"name": "remove_item_ids",
			"type": "Number[]",
			"description": "需要修改的关联关系的商品",
			"maxListSize": 20
		},
		{
			"name": "operator",
			"type": "Json",
			"description": "操作人信息（暂时不填）"
		}
	],
	"responseParams": [
		{
			"name": "result",
			"type": "Result",
			"description": "result",
			"subParams": [
				{
					"name": "message",
					"type": "String",
					"description": "提示信息",
					"required": true
				},
				{
					"name": "code",
					"type": "String",
					"description": "状态码：0 成功，CATEGORY_NOT_FOUND 类目找不到，编辑失败，CATEGORY_INNER_NAME_USED 类目内部名称已被使用，CATEGORY_MAX_EXCEEDED 类目数量已达到上限100，OPERATOR_ITEM_LINK_FORBIDDEN 商品不属于当前卖家，不允许操作",
					"required": true
				}
			]
		}
	]
}