{
	"name": "taobao.simba.rpt.campaignbase.get",
	"apiChineseName": "推广计划报表基础数据对象",
	"description": "推广计划报表基础数据对象",
	"requestParams": [
		{
			"name": "subway_token",
			"type": "String",
			"description": "权限校验参数",
			"required": true
		},
		{
			"name": "nick",
			"type": "String",
			"description": "昵称"
		},
		{
			"name": "start_time",
			"type": "String",
			"description": "开始时间，格式yyyy-mm-dd",
			"required": true
		},
		{
			"name": "end_time",
			"type": "String",
			"description": "结束时间，格式yyyy-mm-dd",
			"required": true
		},
		{
			"name": "search_type",
			"type": "String",
			"description": "报表类型（搜索：SEARCH,类目出价：CAT, 定向投放：NOSEARCH 全部：ALL）可以一次取多个例如：SEARCH,CAT",
			"required": true
		},
		{
			"name": "source",
			"type": "String",
			"description": "数据来源（站内：1，站外：2）可多选以逗号分隔，默认值为：1,2",
			"required": true
		},
		{
			"name": "page_no",
			"type": "Number",
			"description": "页码",
			"minValue": 1
		},
		{
			"name": "page_size",
			"type": "Number",
			"description": "每页大小",
			"minValue": 1
		},
		{
			"name": "campaign_id",
			"type": "Number",
			"description": "推广计划id",
			"required": true
		}
	],
	"responseParams": [
		{
			"name": "rpt_campaign_base_list",
			"type": "String",
			"description": "推广计划查询结果"
		}
	]
}