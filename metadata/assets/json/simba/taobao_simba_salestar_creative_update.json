{
	"name": "taobao.simba.salestar.creative.update",
	"apiChineseName": "销量明星更新创意相关接口",
	"description": "更新一个创意的信息，可以设置创意标题、创意图片",
	"requestParams": [
		{
			"name": "adgroup_id",
			"type": "Number",
			"description": "推广组Id",
			"required": true
		},
		{
			"name": "creative_id",
			"type": "Number",
			"description": "创意Id",
			"required": true
		},
		{
			"name": "title",
			"type": "String",
			"description": "创意标题，最多20个汉字",
			"required": true,
			"maxLength": 40
		},
		{
			"name": "img_url",
			"type": "String",
			"description": "创意图片地址，必须是推广组对应商品的图片之一",
			"required": true
		},
		{
			"name": "picture_id",
			"type": "Number",
			"description": "如果用户开通了创意本地上传图片功能的，可以使用该用户图片空间的图片来修改创意，pictureId为图片空间中图片的pictureId，img_url为图片空间中图片链接地址，如果是使用的主图或副图修改创意，则pictureId必须为空"
		}
	],
	"responseParams": [
		{
			"name": "creativerecord",
			"type": "CreativeRecord",
			"description": "创意修改记录对象",
			"subParams": [
				{
					"name": "nick",
					"type": "String",
					"description": "主人昵称"
				},
				{
					"name": "creative_id",
					"type": "Number",
					"description": "关键词id"
				},
				{
					"name": "title",
					"type": "String",
					"description": "创意标题，最多20个汉字"
				},
				{
					"name": "old_title",
					"type": "String",
					"description": "修改前创意标题"
				},
				{
					"name": "img_url",
					"type": "String",
					"description": "创意图片地址，必须是推广组对应商品的图片之一"
				},
				{
					"name": "old_img_url",
					"type": "String",
					"description": "修改前创意图片地址"
				},
				{
					"name": "audit_status",
					"type": "String",
					"description": "审核状态： audit_wait-待审核；audit_pass-审核通过(上线)；audit_reject-审核拒绝；默认为audit_pass。"
				},
				{
					"name": "audit_desc",
					"type": "String",
					"description": "审核拒绝原因描述"
				},
				{
					"name": "modify_time",
					"type": "Date",
					"description": "创意被修改的时间"
				},
				{
					"name": "create_time",
					"type": "Date",
					"description": "创建时间"
				}
			]
		}
	]
}