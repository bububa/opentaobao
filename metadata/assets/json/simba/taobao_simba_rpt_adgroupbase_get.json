{
	"name": "taobao.simba.rpt.adgroupbase.get",
	"apiChineseName": "推广组基础报表数据对象",
	"description": "推广组基础报表数据对象",
	"requestParams": [
		{
			"name": "subway_token",
			"type": "String",
			"description": "权限校验参数",
			"required": true
		},
		{
			"name": "nick",
			"type": "String",
			"description": "昵称"
		},
		{
			"name": "campaign_id",
			"type": "Number",
			"description": "推广计划id",
			"required": true
		},
		{
			"name": "adgroup_id",
			"type": "Number",
			"description": "推广组id",
			"required": true
		},
		{
			"name": "start_time",
			"type": "String",
			"description": "开始时间，格式yyyy-mm-dd",
			"required": true
		},
		{
			"name": "end_time",
			"type": "String",
