{
	"name": "taobao.simba.rpt.custbase.get",
	"apiChineseName": "客户账户报表基础数据对象",
	"description": "客户账户报表基础数据对象",
	"requestParams": [
		{
			"name": "subway_token",
			"type": "String",
			"description": "权限验证信息",
			"required": true
		},
		{
			"name": "nick",
			"type": "String",
			"description": "昵称"
		},
		{
			"name": "start_time",
			"type": "String",
			"description": "开始日期，格式yyyy-mm-dd",
			"required": true
		},
		{
			"name": "end_time",
			"type": "String",
			"description": "结束日期，格式yyyy-mm-dd",
			"required": true
		},
		{
			"name": "page_no",
			"type": "Number",
			"description": "页码",
			"minValue": 1
		},
		{
			"name": "page_size",
			"type": "Number",
			"description": "每页大小",
			"minValue": 1
		},
		{
			"name": "source",
			"type": "String",
			"description": "数据来源（站内：1，站外：2 ，汇总：SUMMARY）SUMMARY必须单选，其他值可多选例如1,2",
			"required": true
		}
	],
	"responseParams": [
		{
			"name": "rpt_cust_base_list",
			"type": "String",
			"description": "用户帐户结果"
		}
	]
}