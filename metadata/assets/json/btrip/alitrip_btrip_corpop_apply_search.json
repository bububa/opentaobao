			"type": "OpenIsvSearchRq",
			"description": "请求对象",
			"required": true,
			"subParams": [
				{
					"name": "all_apply",
					"type": "Boolean",
					"description": "false:未报销的申请单"
				},
				{
					"name": "corp_id",
					"type": "String",
					"description": "第三方企业id",
					"required": true
				},
				{
					"name": "depart_id",
					"type": "String",
					"description": "申请人所在部门id"
				},
				{
					"name": "end_time",
					"type": "Date",
					"description": "结束时间"
				},
				{
					"name": "gmt_modified",
					"type": "Date",
					"description": "更新时间大于等于此时间的审批单"
				},
				{
					"name": "only_shang_lv_apply",
					"type": "Boolean",
					"description": "true:商旅申请单"
				},
				{
					"name": "page",
					"type": "Number",
					"description": "页数，从1开始"
				},
				{
					"name": "page_size",
					"type": "Number",
					"description": "每页返回数量，默认10，最多50"
				},
				{
					"name": "start_time",
					"type": "Date",
					"description": "开始时间"
				},
				{
					"name": "user_id",
					"type": "String",
					"description": "申请人Id（第三方用户id）"
				},
				{
					"name": "version",
					"type": "Number",
					"description": "1、老版本2、isv对外版本"
				},
				{
					"name": "type",
					"type": "Number",
					"description": "申请单提交类型，1代提交 2本人提交 注意：当申请单为代提交时，申请单提交人自己无法为自己下单"
