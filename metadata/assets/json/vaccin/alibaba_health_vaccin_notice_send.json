{
	"name": "alibaba.health.vaccin.notice.send",
	"apiChineseName": "发送消息提醒",
	"description": "ISV 通过免疫规划中心给用户发送短信或者支付宝 PUSH 提醒。",
	"requestParams": [
		{
			"name": "alipay_user_id",
			"type": "String",
			"description": "支付宝用户 ID",
			"required": true
		},
		{
			"name": "isv_user_id",
			"type": "String",
			"description": "ISV 侧用户 ID"
		},
		{
			"name": "order_id",
			"type": "String",
			"description": "订单 ID"
		},
		{
			"name": "name",
			"type": "String",
			"description": "接种人姓名",
			"required": true
		},
		{
			"name": "mobile",
			"type": "String",
			"description": "联系电话"
		},
		{
			"name": "vaccine_list",
			"type": "VaccineInfo[]",
			"description": "接种的疫苗信息",
			"required": true,
			"maxListSize": 999,
			"subParams": [
				{
					"name": "vaccine_name",
					"type": "String",
					"description": "疫苗名称",
					"required": true
				},
				{
					"name": "vaccine_code",
					"type": "String",
					"description": "疫苗编码"
				},
				{
					"name": "the_times",
					"type": "Number",
					"description": "疫苗针次",
					"required": true
				},
				{
					"name": "vaccine_gb_code",
					"type": "String",
					"description": "阿里健康疫苗编码",
					"required": true
				}
			]
		},
		{
			"name": "sex",
			"type": "Number",
			"description": "接种人性别:1=男,2=女"
		},
		{
			"name": "birthday",
			"type": "String",
			"description": "接种人出生日期"
		},
		{
			"name": "pov_no",
			"type": "String",
			"description": "接种点编码"
		},
		{
			"name": "pov_name",
			"type": "String",
			"description": "接种点名称",
			"required": true
		},
		{
			"name": "address",
			"type": "String",
			"description": "接种点地址"
		},
		{
			"name": "province",
			"type": "String",
			"description": "省份名称"
		},
		{
			"name": "city",
			"type": "String",
			"description": "城市名称"
		},
		{
			"name": "area",
			"type": "String",
			"description": "区名称"
		},
		{
			"name": "reserve_date",
			"type": "String",
			"description": "预约日期"
		},
		{
			"name": "reserve_time",
			"type": "String",
			"description": "预约时间段"
		},
		{
			"name": "message_type",
			"type": "Number",
			"description": "消息提醒类型：1=疫苗下一针预约提醒  2=预约成功提醒  3=接种提醒  4=补种提醒",
			"required": true
		},
		{
			"name": "app_channel",
			"type": "String",
			"description": "用户入口，支付宝或医鹿，alipay或yl"
		}
	],
	"responseParams": [
		{
			"name": "is_success",
			"type": "Boolean",
			"description": "是否成功执行"
		},
		{
			"name": "model",
			"type": "Boolean",
			"description": "有数据返回时的数据详情"
		},
		{
			"name": "msg_info",
			"type": "String",
			"description": "找不到疫苗信息"
		},
		{
			"name": "msg_code",
			"type": "String",
			"description": "200"
		}
	]
}